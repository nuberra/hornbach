        (länge <= 80 And breite <= 120 And tiefe <= 250 And gewicht <= 800) Then
        versandkostenkennzeichen = 53
        versandart = 30
        Exit Sub
    End If
    
    ' Versandkostenkennzeichen 33 und Versandart 30
    If (länge <= 400 And breite <= 240 And tiefe <= 160 And gewicht <= 1600) Or _
        (länge <= 400 And breite <= 160 And tiefe <= 240 And gewicht <= 1600) Or _
        (länge <= 240 And breite <= 400 And tiefe <= 160 And gewicht <= 1600) Or _
        (länge <= 160 And breite <= 400 And tiefe <= 240 And gewicht <= 1600) Or _
        (länge <= 240 And breite <= 160 And tiefe <= 400 And gewicht <= 1600) Or _
        (länge <= 160 And breite <= 240 And tiefe <= 400 And gewicht <= 1600) Then
        versandkostenkennzeichen = 33
        versandart = 30
        Exit Sub
    End If

    ' Versandkostenkennzeichen 64 und Versandart 30
    If (länge <= 500 And breite <= 360 And tiefe <= 240 And gewicht <= 2400) Or _
        (länge <= 500 And breite <= 240 And tiefe <= 360 And gewicht <= 2400) Or _
        (länge <= 360 And breite <= 500 And tiefe <= 240 And gewicht <= 2400) Or _
        (länge <= 240 And breite <= 500 And tiefe <= 360 And gewicht <= 2400) Or _
        (länge <= 360 And breite <= 240 And tiefe <= 500 And gewicht <= 2400) Or _
        (länge <= 240 And breite <= 360 And tiefe <= 500 And gewicht <= 2400) Then
        versandkostenkennzeichen = 64
        versandart = 30
        Exit Sub
    End If
End Sub

