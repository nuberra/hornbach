        (länge <= 200 And breite <= 150 And tiefe <= 200 And gewicht <= 800) Or _
        (länge <= 200 And breite <= 200 And tiefe <= 150 And gewicht <= 800) Or _
        (länge <= 200 And breite <= 200 And tiefe <= 150 And gewicht <= 800) Then
        versandkostenkennzeichen = 53
        versandart = 30
        Exit Sub
    End If

    ' Versandkostenkennzeichen 33 und Versandart 30
    If (länge <= 80 And breite <= 200 And tiefe <= 250 And gewicht <= 900) Or _
        (länge <= 80 And breite <= 200 And tiefe <= 250 And gewicht <= 900) Or _
        (länge <= 250 And breite <= 80 And tiefe <= 200 And gewicht <= 900) Or _
        (länge <= 250 And breite <= 80 And tiefe <= 200 And gewicht <= 900) Or _
