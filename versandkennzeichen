        (länge <= 80 And breite <= 350 And tiefe <= 200 And gewicht <= 900) Or _
        (länge <= 200 And breite <= 80 And tiefe <= 350 And gewicht <= 900) Or _
        (länge <= 200 And breite <= 80 And tiefe <= 350 And gewicht <= 900) Then
        versandkostenkennzeichen = 64
        versandart = 31
        Exit Sub
    End If
End Sub

Sub VAL3_BestimmeVersandkostenBadewannen(länge As Double, breite As Double, tiefe As Double, gewicht As Double, ByRef versandkostenkennzeichen As Integer, ByRef versandart As Integer)
    ' Versandkostenkennzeichen 53 und Versandart 30
    If (länge <= 150 And breite <= 200 And tiefe <= 200 And gewicht <= 800) Or _
        (länge <= 150 And breite <= 200 And tiefe <= 200 And gewicht <= 800) Or _
        (länge <= 200 And breite <= 150 And tiefe <= 200 And gewicht <= 800) Or _
        (länge <= 200 And breite <= 150 And tiefe <= 200 And gewicht <= 800) Or _
        (länge <= 200 And breite <= 200 And tiefe <= 150 And gewicht <= 800) Or _
        (länge <= 200 And breite <= 200 And tiefe <= 150 And gewicht <= 800) Then
        versandkostenkennzeichen = 53
        versandart = 31
        Exit Sub
    End If

    ' Versandkostenkennzeichen 33 und Versandart 30
    If (länge <= 80 And breite <= 200 And tiefe <= 250 And gewicht <= 900) Or _
        (länge <= 80 And breite <= 200 And tiefe <= 250 And gewicht <= 900) Or _
        (länge <= 250 And breite <= 80 And tiefe <= 200 And gewicht <= 900) Or _
        (länge <= 250 And breite <= 80 And tiefe <= 200 And gewicht <= 900) Or _
        (länge <= 200 And breite <= 250 And tiefe <= 80 And gewicht <= 900) Or _
        (länge <= 200 And breite <= 250 And tiefe <= 80 And gewicht <= 900) Then
        versandkostenkennzeichen = 33
        versandart = 31
        Exit Sub
    End If

    ' Versandkostenkennzeichen 64 und Versandart 30
        If (länge <= 80 And breite <= 200 And tiefe <= 350 And gewicht <= 900) Or _
        (länge <= 80 And breite <= 200 And tiefe <= 350 And gewicht <= 900) Or _
        (länge <= 350 And breite <= 80 And tiefe <= 200 And gewicht <= 900) Or _
        (länge <= 350 And breite <= 80 And tiefe <= 200 And gewicht <= 900) Or _
        (länge <= 200 And breite <= 350 And tiefe <= 80 And gewicht <= 900) Or _
        (länge <= 200 And breite <= 350 And tiefe <= 80 And gewicht <= 900) Then
        versandkostenkennzeichen = 64
        versandart = 31
        Exit Sub
    End If
End Sub

Sub VAL3_BestimmeVersandkostenSpiegel(länge As Double, breite As Double, tiefe As Double, gewicht As Double, ByRef versandkostenkennzeichen As Integer, ByRef versandart As Integer)
    ' Versandkostenkennzeichen 51 und Versandart 11
    If (länge <= 98 And breite <= 60 And tiefe <= 60 And gewicht <= 2) Or _
        (länge <= 98 And breite <= 60 And tiefe <= 60 And gewicht <= 2) Or _
        (länge <= 60 And breite <= 98 And tiefe <= 60 And gewicht <= 2) Or _
        (länge <= 60 And breite <= 98 And tiefe <= 60 And gewicht <= 2) Or _
        (länge <= 60 And breite <= 60 And tiefe <= 98 And gewicht <= 2) Or _
        (länge <= 60 And breite <= 60 And tiefe <= 98 And gewicht <= 2) Then
        versandkostenkennzeichen = 51
        versandart = 11
        Exit Sub
    End If

    ' Versandkostenkennzeichen 31 und Versandart 31
    If (länge <= 40 And breite <= 60 And tiefe <= 60 And gewicht <= 100) Or _
        (länge <= 40 And breite <= 60 And tiefe <= 60 And gewicht <= 100) Or _
        (länge <= 60 And breite <= 40 And tiefe <= 60 And gewicht <= 100) Or _
        (länge <= 60 And breite <= 40 And tiefe <= 60 And gewicht <= 100) Or _
        (länge <= 60 And breite <= 60 And tiefe <= 40 And gewicht <= 100) Or _
        (länge <= 60 And breite <= 60 And tiefe <= 40 And gewicht <= 100) Then
        versandkostenkennzeichen = 31
        versandart = 31
        Exit Sub
    End If

    ' Versandkostenkennzeichen 32 und Versandart 31
    If (länge <= 80 And breite <= 80 And tiefe <= 60 And gewicht <= 300) Or _
        (länge <= 80 And breite <= 80 And tiefe <= 60 And gewicht <= 300) Or _
        (länge <= 60 And breite <= 80 And tiefe <= 80 And gewicht <= 300) Or _
        (länge <= 60 And breite <= 80 And tiefe <= 80 And gewicht <= 300) Or _
        (länge <= 80 And breite <= 60 And tiefe <= 80 And gewicht <= 300) Or _
        (länge <= 80 And breite <= 60 And tiefe <= 80 And gewicht <= 300) Then
        versandkostenkennzeichen = 32
        versandart = 31
        Exit Sub
    End If

    ' Versandkostenkennzeichen 53 und Versandart 31
    If (länge <= 120 And breite <= 120 And tiefe <= 80 And gewicht <= 800) Or _
        (länge <= 120 And breite <= 120 And tiefe <= 80 And gewicht <= 800) Or _
        (länge <= 80 And breite <= 120 And tiefe <= 120 And gewicht <= 800) Or _
        (länge <= 80 And breite <= 120 And tiefe <= 120 And gewicht <= 800) Or _
        (länge <= 120 And breite <= 80 And tiefe <= 120 And gewicht <= 800) Or _
        (länge <= 120 And breite <= 80 And tiefe <= 120 And gewicht <= 800) Then
        versandkostenkennzeichen = 53
        versandart = 31
        Exit Sub
    End If

    ' Versandkostenkennzeichen 33 und Versandart 31
    ' Erste Regel: 80 x 240 x 120 mit Gewicht <= 900
    ' Zweite Regel: 160 x 120 x 120 mit Gewicht <= 900
    If (länge <= 80 And breite <= 120 And tiefe <= 240 And gewicht <= 900) Or _
        (länge <= 80 And breite <= 240 And tiefe <= 120 And gewicht <= 900) Or _
        (länge <= 120 And breite <= 80 And tiefe <= 240 And gewicht <= 900) Or _
        (länge <= 120 And breite <= 240 And tiefe <= 80 And gewicht <= 900) Or _
        (länge <= 240 And breite <= 80 And tiefe <= 120 And gewicht <= 900) Or _
        (länge <= 240 And breite <= 120 And tiefe <= 80 And gewicht <= 900) Or _
        (länge <= 160 And breite <= 120 And tiefe <= 120 And gewicht <= 900) Or _
        (länge <= 120 And breite <= 160 And tiefe <= 120 And gewicht <= 900) Or _
        (länge <= 120 And breite <= 120 And tiefe <= 160 And gewicht <= 900) Then
        versandkostenkennzeichen = 33
        versandart = 31
        Exit Sub
    End If
End Sub

Sub VAL3_BestimmeVersandkostenBetonplatten(länge As Double, breite As Double, tiefe As Double, gewicht As Double, ByRef versandkostenkennzeichen As Integer, ByRef versandart As Integer)
    ' Versandkostenkennzeichen 53 und Versandart 31
    If (länge <= 80 And breite <= 120 And tiefe <= 120 And gewicht <= 800) Or _
        (länge <= 80 And breite <= 120 And tiefe <= 120 And gewicht <= 800) Or _
        (länge <= 120 And breite <= 80 And tiefe <= 120 And gewicht <= 800) Or _
        (länge <= 120 And breite <= 80 And tiefe <= 120 And gewicht <= 800) Or _
        (länge <= 120 And breite <= 120 And tiefe <= 80 And gewicht <= 800) Or _
        (länge <= 120 And breite <= 120 And tiefe <= 80 And gewicht <= 800) Then
        versandkostenkennzeichen = 53
        versandart = 31
        Exit Sub
    End If

    ' Versandkostenkennzeichen 33 und Versandart 31
    ' Erste Regel: 80 x 240 x 120 mit Gewicht <= 900
    ' Zweite Regel: 160 x 120 x 120 mit Gewicht <= 900
    If (länge <= 80 And breite <= 120 And tiefe <= 240 And gewicht <= 900) Or _
        (länge <= 80 And breite <= 240 And tiefe <= 120 And gewicht <= 900) Or _
        (länge <= 120 And breite <= 80 And tiefe <= 240 And gewicht <= 900) Or _
        (länge <= 120 And breite <= 240 And tiefe <= 80 And gewicht <= 900) Or _
        (länge <= 240 And breite <= 80 And tiefe <= 120 And gewicht <= 900) Or _
        (länge <= 240 And breite <= 120 And tiefe <= 80 And gewicht <= 900) Or _
        (länge <= 160 And breite <= 120 And tiefe <= 120 And gewicht <= 900) Or _
        (länge <= 120 And breite <= 160 And tiefe <= 120 And gewicht <= 900) Or _
        (länge <= 120 And breite <= 120 And tiefe <= 160 And gewicht <= 900) Then
        versandkostenkennzeichen = 33
        versandart = 31
        Exit Sub
    End If
End Sub

Sub VAL3_BestimmeVersandkostenKühlschränke(länge As Double, breite As Double, tiefe As Double, gewicht As Double, ByRef versandkostenkennzeichen As Integer, ByRef versandart As Integer)
    ' Versandkennzeichen 31 und Versandart 31
    If (länge <= 40 And breite <= 120 And tiefe <= 60 And gewicht <= 100) Or _
        (länge <= 40 And breite <= 120 And tiefe <= 60 And gewicht <= 100) Or _
        (länge <= 60 And breite <= 40 And tiefe <= 120 And gewicht <= 100) Or _
        (länge <= 60 And breite <= 40 And tiefe <= 120 And gewicht <= 100) Or _
        (länge <= 120 And breite <= 60 And tiefe <= 40 And gewicht <= 100) Or _
        (länge <= 120 And breite <= 60 And tiefe <= 40 And gewicht <= 100) Then
        versandkostenkennzeichen = 31
        versandart = 31
        Exit Sub
    End If

    'Versandkennzeichen 32 und Versandart 31
    If (länge <= 80 And breite <= 180 And tiefe <= 60 And gewicht <= 300) Or _
        (länge <= 80 And breite <= 180 And tiefe <= 60 And gewicht <= 300) Or _
        (länge <= 60 And breite <= 80 And tiefe <= 180 And gewicht <= 300) Or _
        (länge <= 60 And breite <= 80 And tiefe <= 180 And gewicht <= 300) Or _
        (länge <= 180 And breite <= 60 And tiefe <= 80 And gewicht <= 300) Or _
        (länge <= 180 And breite <= 60 And tiefe <= 80 And gewicht <= 300) Then
        versandkostenkennzeichen = 32
        versandart = 31
        Exit Sub
    End If

    'Versandkennzeichen 30 und Versandart 31
    If (länge <= 80 And breite <= 200 And tiefe <= 120 And gewicht <= 800) Or _
        (länge <= 80 And breite <= 200 And tiefe <= 120 And gewicht <= 800) Or _
        (länge <= 120 And breite <= 80 And tiefe <= 200 And gewicht <= 800) Or _
        (länge <= 120 And breite <= 80 And tiefe <= 200 And gewicht <= 800) Or _
        (länge <= 200 And breite <= 120 And tiefe <= 80 And gewicht <= 800) Or _
        (länge <= 200 And breite <= 120 And tiefe <= 80 And gewicht <= 800) Then
        versandkostenkennzeichen = 30
        versandart = 31
        Exit Sub
    End If

    'Versandkennzeichen 33 und Versandart 31
    'Erste Regel: 80 x 200 x 120 cm
    'Zweite Regel: 160 x 120 x 120 cm
    If (länge <= 80 And breite <= 120 And tiefe <= 200 And gewicht <= 900) Or _
        (länge <= 80 And breite <= 200 And tiefe <= 120 And gewicht <= 900) Or _
        (länge <= 120 And breite <= 80 And tiefe <= 200 And gewicht <= 900) Or _
        (länge <= 120 And breite <= 200 And tiefe <= 80 And gewicht <= 900) Or _
        (länge <= 200 And breite <= 80 And tiefe <= 120 And gewicht <= 900) Or _
        (länge <= 200 And breite <= 120 And tiefe <= 80 And gewicht <= 900) Or _
        (länge <= 120 And breite <= 120 And tiefe <= 160 And gewicht <= 900) Or _
        (länge <= 160 And breite <= 120 And tiefe <= 120 And gewicht <= 900) Or _
        (länge <= 120 And breite <= 160 And tiefe <= 120 And gewicht <= 900) Then
        versandkostenkennzeichen = 33
        versandart = 31
        Exit Sub
    End If

    'Versandkennzeichen 64 und Versandart 31
    If (länge <= 80 And breite <= 200 And tiefe <= 360 And gewicht <= 1300) Or _
        (länge <= 80 And breite <= 200 And tiefe <= 360 And gewicht <= 1300) Or _
        (länge <= 360 And breite <= 80 And tiefe <= 200 And gewicht <= 1300) Or _
        (länge <= 360 And breite <= 80 And tiefe <= 200 And gewicht <= 1300) Or _
        (länge <= 200 And breite <= 360 And tiefe <= 80 And gewicht <= 1300) Or _
        (länge <= 200 And breite <= 360 And tiefe <= 80 And gewicht <= 1300) Then
        versandkostenkennzeichen = 64
        versandart = 31
        Exit Sub
    End If

    'Versandkennzeichen 65 und Versandart 31
    If (länge <= 2400 And breite <= 480 And tiefe <= 320 And gewicht <= 3200) Or _
        (länge <= 2400 And breite <= 480 And tiefe <= 320 And gewicht <= 3200) Or _
        (länge <= 320 And breite <= 2400 And tiefe <= 480 And gewicht <= 3200) Or _
        (länge <= 320 And breite <= 2400 And tiefe <= 480 And gewicht <= 3200) Or _
        (länge <= 480 And breite <= 320 And tiefe <= 2400 And gewicht <= 3200) Or _
        (länge <= 480 And breite <= 320 And tiefe <= 2400 And gewicht <= 3200) Then
        versandkostenkennzeichen = 65
        versandart = 31
        Exit Sub
    End If
End Sub

