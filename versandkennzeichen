    ' Versandkostenkennzeichen 51 und Versandart 10
    If gewicht <= 30 And ((länge <= 98 And breite <= 60 And tiefe <= 60) Or _
                          (breite <= 98 And länge <= 60 And tiefe <= 60) Or _
                          (tiefe <= 98 And länge <= 60 And breite <= 60)) Then
        versandkostenkennzeichen = 51
        versandart = 10
        Exit Sub
    End If
        
    ' Versandkostenkennzeichen 53 und Versandart 30
    If (länge <= 250 And breite <= 120 And tiefe <= 80 And gewicht <= 800) Or _
        (länge <= 250 And breite <= 80 And tiefe <= 120 And gewicht <= 800) Or _
        (länge <= 120 And breite <= 250 And tiefe <= 80 And gewicht <= 800) Or _
        (länge <= 80 And breite <= 250 And tiefe <= 120 And gewicht <= 800) Or _
