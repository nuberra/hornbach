Sub MVZ_BestimmeVersandkostenKühlschränke(länge As Double, breite As Double, tiefe As Double, gewicht As Double, ByRef versandkostenkennzeichen As Integer, ByRef versandart As Integer)
    ' Versandkennzeichen 31 und Versandart 30
    If (länge <= 40 And breite <= 120 And tiefe <= 60 And gewicht <= 100) Or _
        (länge <= 40 And breite <= 120 And tiefe <= 60 And gewicht <= 100) Or _
        (länge <= 60 And breite <= 40 And tiefe <= 120 And gewicht <= 100) Or _
        (länge <= 60 And breite <= 40 And tiefe <= 120 And gewicht <= 100) Or _
        (länge <= 120 And breite <= 60 And tiefe <= 40 And gewicht <= 100) Or _
        (länge <= 120 And breite <= 60 And tiefe <= 40 And gewicht <= 100) Then
        versandkostenkennzeichen = 31
        versandart = 30
        Exit Sub
    End If

    'Versandkennzeichen 32 und Versandart 30
    If (länge <= 80 And breite <= 180 And tiefe <= 60 And gewicht <= 300) Or _
        (länge <= 80 And breite <= 180 And tiefe <= 60 And gewicht <= 300) Or _
        (länge <= 60 And breite <= 80 And tiefe <= 180 And gewicht <= 300) Or _
        (länge <= 60 And breite <= 80 And tiefe <= 180 And gewicht <= 300) Or _
        (länge <= 180 And breite <= 60 And tiefe <= 80 And gewicht <= 300) Or _
        (länge <= 180 And breite <= 60 And tiefe <= 80 And gewicht <= 300) Then
        versandkostenkennzeichen = 32
        versandart = 30
        Exit Sub
    End If

    'Versandkennzeichen 30 und Versandart 30
    If (länge <= 80 And breite <= 200 And tiefe <= 120 And gewicht <= 800) Or _
        (länge <= 80 And breite <= 200 And tiefe <= 120 And gewicht <= 800) Or _
        (länge <= 120 And breite <= 80 And tiefe <= 200 And gewicht <= 800) Or _
        (länge <= 120 And breite <= 80 And tiefe <= 200 And gewicht <= 800) Or _
        (länge <= 200 And breite <= 120 And tiefe <= 80 And gewicht <= 800) Or _
        (länge <= 200 And breite <= 120 And tiefe <= 80 And gewicht <= 800) Then
        versandkostenkennzeichen = 30
        versandart = 30
        Exit Sub
    End If

    'Versandkennzeichen 33 und Versandart 30
    'Erste Regel: 80 x 200 x 120 cm
    'Zweite Regel: 160 x 120 x 120 cm
    If (länge <= 80 And breite <= 120 And tiefe <= 200 And gewicht <= 900) Or _
        (länge <= 80 And breite <= 200 And tiefe <= 120 And gewicht <= 900) Or _
        (länge <= 120 And breite <= 80 And tiefe <= 200 And gewicht <= 900) Or _
        (länge <= 120 And breite <= 200 And tiefe <= 80 And gewicht <= 900) Or _
        (länge <= 200 And breite <= 80 And tiefe <= 120 And gewicht <= 900) Or _
        (länge <= 200 And breite <= 120 And tiefe <= 80 And gewicht <= 900) Or _
        (länge <= 120 And breite <= 120 And tiefe <= 160 And gewicht <= 900) Or _
        (länge <= 160 And breite <= 120 And tiefe <= 120 And gewicht <= 900) Or _
        (länge <= 120 And breite <= 160 And tiefe <= 120 And gewicht <= 900) Then
        versandkostenkennzeichen = 33
        versandart = 30
        Exit Sub
    End If

    'Versandkennzeichen 64 und Versandart 30
    If (länge <= 80 And breite <= 200 And tiefe <= 360 And gewicht <= 1300) Or _
        (länge <= 80 And breite <= 200 And tiefe <= 360 And gewicht <= 1300) Or _
        (länge <= 360 And breite <= 80 And tiefe <= 200 And gewicht <= 1300) Or _
        (länge <= 360 And breite <= 80 And tiefe <= 200 And gewicht <= 1300) Or _
        (länge <= 200 And breite <= 360 And tiefe <= 80 And gewicht <= 1300) Or _
        (länge <= 200 And breite <= 360 And tiefe <= 80 And gewicht <= 1300) Then
        versandkostenkennzeichen = 64
        versandart = 30
        Exit Sub
    End If

    'Versandkennzeichen 65 und Versandart 30
    If (länge <= 2400 And breite <= 480 And tiefe <= 320 And gewicht <= 3200) Or _
        (länge <= 2400 And breite <= 480 And tiefe <= 320 And gewicht <= 3200) Or _
        (länge <= 320 And breite <= 2400 And tiefe <= 480 And gewicht <= 3200) Or _
        (länge <= 320 And breite <= 2400 And tiefe <= 480 And gewicht <= 3200) Or _
        (länge <= 480 And breite <= 320 And tiefe <= 2400 And gewicht <= 3200) Or _
        (länge <= 480 And breite <= 320 And tiefe <= 2400 And gewicht <= 3200) Then
        versandkostenkennzeichen = 65
        versandart = 30
        Exit Sub
    End If
End Sub

Sub FFDL_BestimmeVersandkosten(länge As Double, breite As Double, tiefe As Double, gewicht As Double, ByRef versandkostenkennzeichen As Integer, ByRef versandart As Integer)
    ' Versandkostenkennzeichen 58 und Versandart 10
    If gewicht <= 30 And ((länge <= 98 And breite <= 60 And tiefe <= 60) Or _
                          (breite <= 98 And länge <= 60 And tiefe <= 60) Or _
                          (tiefe <= 98 And länge <= 60 And breite <= 60)) Then
        versandkostenkennzeichen = 58
        versandart = 10
        Exit Sub
    End If
    
    ' Versandkostenkennzeichen 57 und Versandart 20
    längste_seite = Application.WorksheetFunction.Max(länge, breite, tiefe)
    gurtmass = 2 * (länge + breite + tiefe - längste_seite)
    If längste_seite <= 250 And gurtmass <= 400 And gewicht <= 30 Then
        versandkostenkennzeichen = 57
        versandart = 20
        Exit Sub
    End If
    
    ' Versandkostenkennzeichen 55 und Versandart 30
    If (länge <= 600 And breite <= 60 And tiefe <= 40 And gewicht <= 100) Or _
       (länge <= 600 And breite <= 40 And tiefe <= 60 And gewicht <= 100) Or _
       (länge <= 40 And breite <= 600 And tiefe <= 60 And gewicht <= 100) Or _
       (länge <= 60 And breite <= 40 And tiefe <= 600 And gewicht <= 100) Or _
       (länge <= 60 And breite <= 600 And tiefe <= 40 And gewicht <= 100) Or _
       (länge <= 40 And breite <= 60 And tiefe <= 600 And gewicht <= 100) Then
        versandkostenkennzeichen = 55
        versandart = 30
        Exit Sub
    End If
    
    ' Versandkostenkennzeichen 0 und Versandart 0
    If (länge <= 600 And breite <= 60 And tiefe <= 80 And gewicht <= 300) Or _
        (länge <= 600 And breite <= 80 And tiefe <= 60 And gewicht <= 300) Or _
        (länge <= 60 And breite <= 600 And tiefe <= 80 And gewicht <= 300) Or _
        (länge <= 80 And breite <= 600 And tiefe <= 60 And gewicht <= 300) Or _
        (länge <= 60 And breite <= 80 And tiefe <= 600 And gewicht <= 300) Or _
        (länge <= 80 And breite <= 60 And tiefe <= 600 And gewicht <= 300) Then
        versandkostenkennzeichen = 0
        versandart = 0
        Exit Sub
    End If
    
    ' Versandkostenkennzeichen 56 und Versandart 30
    ' Erste Regel: 40 x 60 x 599 mit Gewicht <= 70
    ' Zweite Regel: 120 x 120 x 80 mit Gewicht <= 800
    If (länge <= 40 And breite <= 60 And tiefe <= 599 And gewicht <= 70) Or _
        (länge <= 40 And breite <= 599 And tiefe <= 60 And gewicht <= 70) Or _
        (länge <= 60 And breite <= 40 And tiefe <= 599 And gewicht <= 70) Or _
        (länge <= 60 And breite <= 599 And tiefe <= 40 And gewicht <= 70) Or _
        (länge <= 599 And breite <= 40 And tiefe <= 60 And gewicht <= 70) Or _
        (länge <= 599 And breite <= 60 And tiefe <= 40 And gewicht <= 70) Or _
        (länge <= 120 And breite <= 120 And tiefe <= 80 And gewicht <= 800) Or _
        (länge <= 80 And breite <= 120 And tiefe <= 120 And gewicht <= 800) Or _
        (länge <= 120 And breite <= 80 And tiefe <= 120 And gewicht <= 800) Then
        versandkostenkennzeichen = 56
        versandart = 30
        Exit Sub
    End If

    ' Versandkostenkennzeichen 13 und Versandart 30
    ' Erste Regel: 80 x 120 x 240 mit Gewicht <= 900
    ' Zweite Regel: 120 x 120 x 160 mit Gewicht <= 900
    If (länge <= 80 And breite <= 120 And tiefe <= 240 And gewicht <= 900) Or _
        (länge <= 80 And breite <= 240 And tiefe <= 80 And gewicht <= 900) Or _
        (länge <= 120 And breite <= 80 And tiefe <= 240 And gewicht <= 900) Or _
        (länge <= 120 And breite <= 240 And tiefe <= 80 And gewicht <= 900) Or _
        (länge <= 240 And breite <= 80 And tiefe <= 120 And gewicht <= 900) Or _
        (länge <= 240 And breite <= 120 And tiefe <= 80 And gewicht <= 900) Or _
        (länge <= 120 And breite <= 120 And tiefe <= 160 And gewicht <= 900) Or _
        (länge <= 160 And breite <= 120 And tiefe <= 120 And gewicht <= 900) Or _
        (länge <= 120 And breite <= 160 And tiefe <= 120 And gewicht <= 900) Then
        versandkostenkennzeichen = 13
        versandart = 30
        Exit Sub
    End If
    
    ' Versandkostenkennzeichen 0 und Versandart 0
    If (länge <= 1200 And breite <= 240 And tiefe <= 160 And gewicht <= 1600) Or _
        (länge <= 1200 And breite <= 160 And tiefe <= 240 And gewicht <= 1600) Or _
        (länge <= 240 And breite <= 1200 And tiefe <= 160 And gewicht <= 1600) Or _
        (länge <= 160 And breite <= 1200 And tiefe <= 240 And gewicht <= 1600) Or _
        (länge <= 240 And breite <= 160 And tiefe <= 1200 And gewicht <= 1600) Or _
        (länge <= 160 And breite <= 240 And tiefe <= 1200 And gewicht <= 1600) Then
        versandkostenkennzeichen = 0
        versandart = 0
        Exit Sub
    End If

    ' Versandkostenkennzeichen 0 und Versandart 0
    If (länge <= 1800 And breite <= 360 And tiefe <= 240 And gewicht <= 2400) Or _
        (länge <= 1800 And breite <= 240 And tiefe <= 360 And gewicht <= 2400) Or _
        (länge <= 360 And breite <= 1800 And tiefe <= 240 And gewicht <= 2400) Or _
        (länge <= 240 And breite <= 1800 And tiefe <= 360 And gewicht <= 2400) Or _
        (länge <= 360 And breite <= 240 And tiefe <= 1800 And gewicht <= 2400) Or _
        (länge <= 240 And breite <= 360 And tiefe <= 1800 And gewicht <= 2400) Then
        versandkostenkennzeichen = 0
        versandart = 0
        Exit Sub
    End If

    ' Versandkostenkennzeichen 0 und Versandart 0
    If (länge <= 2400 And breite <= 480 And tiefe <= 320 And gewicht <= 3200) Or _
        (länge <= 2400 And breite <= 320 And tiefe <= 480 And gewicht <= 3200) Or _
        (länge <= 480 And breite <= 2400 And tiefe <= 320 And gewicht <= 3200) Or _
        (länge <= 320 And breite <= 2400 And tiefe <= 480 And gewicht <= 3200) Or _
        (länge <= 480 And breite <= 320 And tiefe <= 2400 And gewicht <= 3200) Or _
        (länge <= 320 And breite <= 480 And tiefe <= 2400 And gewicht <= 3200) Then
        versandkostenkennzeichen = 0
        versandart = 0
        Exit Sub
    End If

    ' Versandkostenkennzeichen 0 und Versandart 0
    If (länge <= 3000 And breite <= 600 And tiefe <= 400 And gewicht <= 4000) Or _
        (länge <= 3000 And breite <= 400 And tiefe <= 600 And gewicht <= 4000) Or _
        (länge <= 600 And breite <= 3000 And tiefe <= 400 And gewicht <= 4000) Or _
        (länge <= 400 And breite <= 3000 And tiefe <= 600 And gewicht <= 4000) Or _
        (länge <= 600 And breite <= 400 And tiefe <= 3000 And gewicht <= 4000) Or _
        (länge <= 400 And breite <= 600 And tiefe <= 3000 And gewicht <= 4000) Then
        versandkostenkennzeichen = 0
        versandart = 0
        Exit Sub
    End If

    ' Versandkostenkennzeichen 0 und Versandart 0
    If (länge <= 3600 And breite <= 720 And tiefe <= 480 And gewicht <= 4800) Or _
        (länge <= 3600 And breite <= 480 And tiefe <= 720 And gewicht <= 4800) Or _
        (länge <= 720 And breite <= 3600 And tiefe <= 480 And gewicht <= 4800) Or _
        (länge <= 480 And breite <= 3600 And tiefe <= 720 And gewicht <= 4800) Or _
        (länge <= 720 And breite <= 480 And tiefe <= 3600 And gewicht <= 4800) Or _
        (länge <= 480 And breite <= 720 And tiefe <= 3600 And gewicht <= 4800) Then
        versandkostenkennzeichen = 0
        versandart = 0
        Exit Sub
    End If
End Sub

Sub FFDL_BestimmeVersandkostenSpezial400CHF(länge As Double, breite As Double, tiefe As Double, gewicht As Double, ByRef versandkostenkennzeichen As Integer, ByRef versandart As Integer)
        ' Versandkostenkennzeichen 58 und Versandart 10
        If gewicht <= 30 And ((länge <= 98 And breite <= 60 And tiefe <= 60) Or _
                            (breite <= 98 And länge <= 60 And tiefe <= 60) Or _
                            (tiefe <= 98 And länge <= 60 And breite <= 60)) Then
            versandkostenkennzeichen = 58
            versandart = 10
            Exit Sub
        End If
    
        ' Versandkostenkennzeichen 57 und Versandart 20
        längste_seite = Application.WorksheetFunction.Max(länge, breite, tiefe)
        gurtmass = 2 * (länge + breite + tiefe - längste_seite)
        If längste_seite <= 250 And gurtmass <= 400 And gewicht <= 30 Then
            versandkostenkennzeichen = 57
            versandart = 20
            Exit Sub
        End If

        ' Versandkostenkennzeichen 80 und Versandart 30
        ' Erste Regel: 40 x 60 x 300 mit Gewicht <= 50
        ' Zweite Regel: 40 x 60 x 60 mit Gewicht <= 100
        If (länge <= 40 And breite <= 60 And tiefe <= 300 And gewicht <= 50) Or _
            (länge <= 40 And breite <= 300 And tiefe <= 60 And gewicht <= 50) Or _
            (länge <= 60 And breite <= 40 And tiefe <= 300 And gewicht <= 50) Or _
            (länge <= 60 And breite <= 300 And tiefe <= 40 And gewicht <= 50) Or _
            (länge <= 300 And breite <= 40 And tiefe <= 60 And gewicht <= 50) Or _
            (länge <= 300 And breite <= 60 And tiefe <= 40 And gewicht <= 50) Or _
            (länge <= 40 And breite <= 60 And tiefe <= 60 And gewicht <= 100) Or _
            (länge <= 60 And breite <= 40 And tiefe <= 60 And gewicht <= 100) Or _
            (länge <= 60 And breite <= 60 And tiefe <= 40 And gewicht <= 100) Then
            versandkostenkennzeichen = 80
            versandart = 30
            Exit Sub
        End If

        ' Versandkostenkennzeichen 81 und Versandart 30
        ' Erste Regel: 40 x 60 x 400 mit Gewicht <= 70
        ' Zweite Regel: 40 x 80 x 80 mit Gewicht <= 300
        If (länge <= 40 And breite <= 60 And tiefe <= 400 And gewicht <= 70) Or _
            (länge <= 40 And breite <= 400 And tiefe <= 60 And gewicht <= 70) Or _
            (länge <= 60 And breite <= 40 And tiefe <= 400 And gewicht <= 70) Or _
            (länge <= 60 And breite <= 400 And tiefe <= 40 And gewicht <= 70) Or _
            (länge <= 400 And breite <= 40 And tiefe <= 60 And gewicht <= 70) Or _
            (länge <= 400 And breite <= 60 And tiefe <= 40 And gewicht <= 70) Or _
            (länge <= 40 And breite <= 80 And tiefe <= 80 And gewicht <= 300) Or _
            (länge <= 80 And breite <= 40 And tiefe <= 80 And gewicht <= 300) Or _
            (länge <= 80 And breite <= 80 And tiefe <= 40 And gewicht <= 300) Then
            versandkostenkennzeichen = 81
            versandart = 30
            Exit Sub
        End If

        ' Versandkostenkennzeichen 82 und Versandart 30
        ' Erste Regel: 40 x 60 x 599 mit Gewicht <= 70
        ' Zweite Regel: 120 x 120 x 80 mit Gewicht <= 800
        If (länge <= 40 And breite <= 60 And tiefe <= 599 And gewicht <= 70) Or _
            (länge <= 40 And breite <= 599 And tiefe <= 60 And gewicht <= 70) Or _
            (länge <= 60 And breite <= 40 And tiefe <= 599 And gewicht <= 70) Or _
            (länge <= 60 And breite <= 599 And tiefe <= 40 And gewicht <= 70) Or _
            (länge <= 599 And breite <= 40 And tiefe <= 60 And gewicht <= 70) Or _
            (länge <= 599 And breite <= 60 And tiefe <= 40 And gewicht <= 70) Or _
            (länge <= 480 And breite <= 720 And tiefe <= 3600 And gewicht <= 900) Or _
            (länge <= 480 And breite <= 3600 And tiefe <= 720 And gewicht <= 900) Or _
            (länge <= 720 And breite <= 480 And tiefe <= 3600 And gewicht <= 900) Or _
            (länge <= 720 And breite <= 3600 And tiefe <= 480 And gewicht <= 900) Or _
            (länge <= 3600 And breite <= 480 And tiefe <= 720 And gewicht <= 900) Or _
            (länge <= 3600 And breite <= 720 And tiefe <= 480 And gewicht <= 900) Then
            versandkostenkennzeichen = 82
            versandart = 30
            Exit Sub
        End If
End Sub

