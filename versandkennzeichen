        (länge <= 120 And breite <= 80 And tiefe <= 120 And gewicht <= 800) Or _
        (länge <= 120 And breite <= 120 And tiefe <= 80 And gewicht <= 800) Or _
        (länge <= 120 And breite <= 120 And tiefe <= 80 And gewicht <= 800) Then
        versandkostenkennzeichen = 30
        versandart = 30
        Exit Sub
    End If

    ' Versandkostenkennzeichen 33 und Versandart 30
    ' Erste Regel: 80 x 240 x 120 mit Gewicht <= 900
    ' Zweite Regel: 160 x 120 x 120 mit Gewicht <= 900
