Sub FFDL_BestimmeVersandkostenFliesen(länge As Double, breite As Double, tiefe As Double, gewicht As Double, ByRef versandkostenkennzeichen As Integer, ByRef versandart As Integer)
    ' Versandkostenkennzeichen 55 und Versandart 30
    If (länge <= 40 And breite <= 60 And tiefe <= 60 And gewicht <= 100) Or _
        (länge <= 40 And breite <= 60 And tiefe <= 60 And gewicht <= 100) Or _
        (länge <= 60 And breite <= 40 And tiefe <= 60 And gewicht <= 100) Or _
        (länge <= 60 And breite <= 40 And tiefe <= 60 And gewicht <= 100) Or _
        (länge <= 60 And breite <= 60 And tiefe <= 40 And gewicht <= 100) Or _
        (länge <= 60 And breite <= 60 And tiefe <= 40 And gewicht <= 100) Then
        versandkostenkennzeichen = 55
        versandart = 30
        Exit Sub
    End If

    ' Versandkostenkennzeichen 56 und Versandart 30
    If (länge <= 124 And breite <= 80 And tiefe <= 80 And gewicht <= 800) Or _
        (länge <= 124 And breite <= 80 And tiefe <= 80 And gewicht <= 800) Or _
        (länge <= 80 And breite <= 124 And tiefe <= 80 And gewicht <= 800) Or _
        (länge <= 80 And breite <= 124 And tiefe <= 80 And gewicht <= 800) Or _
        (länge <= 80 And breite <= 80 And tiefe <= 124 And gewicht <= 800) Or _
        (länge <= 80 And breite <= 80 And tiefe <= 124 And gewicht <= 800) Then
        versandkostenkennzeichen = 56
        versandart = 30
        Exit Sub
    End If

    ' Versandkostenkennzeichen 13 und Versandart 30
    If (länge <= 80 And breite <= 200 And tiefe <= 200 And gewicht <= 900) Or _
        (länge <= 80 And breite <= 200 And tiefe <= 200 And gewicht <= 900) Or _
        (länge <= 200 And breite <= 80 And tiefe <= 200 And gewicht <= 900) Or _
        (länge <= 200 And breite <= 80 And tiefe <= 200 And gewicht <= 900) Or _
        (länge <= 200 And breite <= 200 And tiefe <= 80 And gewicht <= 900) Or _
        (länge <= 200 And breite <= 200 And tiefe <= 80 And gewicht <= 900) Then
        versandkostenkennzeichen = 13
        versandart = 30
        Exit Sub
    End If
End Sub

