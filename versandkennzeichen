    If (länge <= 350 And breite <= 200 And tiefe <= 80 And gewicht <= 900) Or _
        (länge <= 350 And breite <= 200 And tiefe <= 80 And gewicht <= 900) Or _
        (länge <= 80 And breite <= 350 And tiefe <= 200 And gewicht <= 900) Or _
        (länge <= 80 And breite <= 350 And tiefe <= 200 And gewicht <= 900) Or _
        (länge <= 200 And breite <= 80 And tiefe <= 350 And gewicht <= 900) Or _
        (länge <= 200 And breite <= 80 And tiefe <= 350 And gewicht <= 900) Then
        versandkostenkennzeichen = 64
        versandart = 30
