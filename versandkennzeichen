        (länge <= 80 And breite <= 200 And tiefe <= 200 And gewicht <= 900) Or _
        (länge <= 200 And breite <= 80 And tiefe <= 200 And gewicht <= 900) Or _
        (länge <= 200 And breite <= 80 And tiefe <= 200 And gewicht <= 900) Or _
