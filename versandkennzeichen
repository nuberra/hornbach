Function IsInArray(valToBeFound As Variant, arr As Variant) As Boolean
    On Error GoTo IsInArrayError
    
    Dim element As Variant
    IsInArray = False
    
    If Not IsArray(arr) Then Exit Function
    
    ' Konvertiere den zu suchenden Wert nur einmal zu Long
    Dim searchValue As Long
    searchValue = CLng(valToBeFound)
    
    For Each element In arr
        ' Füge zusätzliche Debug-Ausgabe hinzu
        Debug.Print "Comparing " & searchValue & " with " & element
        
        ' Explizite Konvertierung des Array-Elements
        Dim elementValue As Long
        elementValue = CLng(element)
        
        If elementValue = searchValue Then
            IsInArray = True
            Debug.Print "Found match: " & valToBeFound & " = " & element
            Exit Function
        End If
    Next element
    
    Exit Function
    
IsInArrayError:
    Debug.Print "Error in IsInArray with value: " & valToBeFound & ". Error: " & Err.Description
    IsInArray = False
End Function

Sub Main_VersandkostenBerechnen()
    Dim inputSheet As Worksheet
    Dim lastRow As Long
    Dim i As Long
    Dim versandweg As String

    ' Arbeitsblatt "Aktuelle Artikeldaten" festlegen
    Set inputSheet = ThisWorkbook.Sheets("Aktuelle Artikeldaten")

    ' Letzte Zeile der Eingabedatei ermitteln
    lastRow = inputSheet.Cells(inputSheet.rows.Count, 1).End(xlUp).Row

    ' Allgemeine Variablen und Arrays definieren
    Dim specialBrands As Variant
    Dim specialArticleGroups50CHF As Variant
    Dim specialArticleGroups400CHF As Variant
    Dim specialArticleGroups700CHF As Variant
    Dim specialArticleGroupsSackware As Variant
    Dim specialArticleGroupsViertelpalette As Variant
    Dim specialArticleGroupsPalette As Variant
    Dim specialArticleGroupsOnlyViertelpalette As Variant
    Dim specialArticleGroupsOnlyHalbpalette As Variant
    Dim specialArticleGroupsSpMarkiesen As Variant
    Dim specialArticleGroupsSpHolz As Variant
    Dim specialBrandsSpTuere As Variant
    Dim specialArticleGroupsSpTuere As Variant
    Dim specialArticleGroupsSpZargen As Variant
    Dim specialKnauf As Variant
    Dim specialGipsplatten As Variant

    ' Listen der speziellen Bedingungen
    specialBrands = Array("Bosch", "Bosch Professional", "Camping Gaz", "Danfoss", "DURAVIT", _
                          "Einhell", "Feller", "Franke", "Gardena", "Geberit", "Grohe", "Grundfos", _
                          "Hansgrohe", "Hikoki", "Intex", "Jonsered", "Kärcher", "Kibernetik", "KWC", _
                          "Laufen", "Makita", "Metabo", "Mica", "MTD", "Outdoorchef", "Philips", _
                          "Scheppach", "Siemens", "Similor", "Sonnenkönig", "Stanley", "Stiga", _
                          "Sun Comfort/Glatz", "Villeroy&Boch", "Weber", "Wolf-Garten", "Worx")

    specialMuster = Array(100107, 101501, 101502, 101503, 101504, 140412, 140620, 120104, 120409, 120730, 122307)

    specialArticleGroups50CHF = Array(94201)

    specialArticleGroups400CHF = Array(94201, 94203, 94204, 94205, 94206, 94207, 94210, 94211, 94212, _
                                       94213, 94214, 94215, 133501, 133502, 133503, 133504, 133505, _
                                       133507, 133508, 133510, 133601, 133602, 133603, 133604, 133606, _
                                       133607, 133608, 133610, 161205, 161206, 161207, 161209, 161216, _
                                       161217, 161219, 161220, 161221, 161222, 161223, 161224, 161225, _
                                       161601, 161602, 161603, 161604, 161606, 161607, 161608, 161609, _
                                       161610, 161611, 161612, 161701, 161702, 161703, 161704, 161705, _
                                       161706, 161707, 161708, 161709, 161710, 161711, 161712, 161714, _
                                       161715, 161717, 161718, 161801, 161802, 161803, 161805, 214301, _
                                       214302, 214303, 214304, 214306, 214307, 214308, 214501, 214502, _
                                       214503, 214505, 214506, 214507, 214508, 214509, 214522, 214527, _
                                       214528, 214601, 214602, 214603, 214604, 214605, 214606, 214801, _
                                       214802, 214803, 214804, 214806, 214807, 214810, 214812, 214813, _
                                       214815, 214816)

    specialArticleGroups700CHF = Array(204401, 204402, 204403, 204404, 204405, 204406, 204407, _
                                       204408, 204409, 204410, 204411)

    specialArticleGroupsSackware = Array(20301, 20302, 20303, 20304, 20305, 20306, 20307, 20310, 20311, 204606)

    specialArticleGroupsViertelpalette = Array(100601, 100602, 100607, 100701, 100706, 100707, 100708, 100710, 100712, 100713, _
                                               101101, 101103, 101401, 101402, 101403, 101404, 120201, 120203, 120206, 120207, _
                                               120208, 120301, 120303, 120305, 120306, 120401, 120402, 120403, 120404, 120405, _
                                               120406, 120407, 120409, 122201, 122202, 122302, 122304, 122307, 122308, 122309, _
                                               122310, 122311, 122312)

    specialArticleGroupsPalette = Array(21001, 21002, 21003, 21004, 21005, 21009, 21010, 21011, 21012, 21013, _
                                        21014, 21016, 21018, 21019, 21020, 101301, 101303, 101304, 101305, 101306, _
                                        101307, 101310, 101311, 101312, 101313, 101314, 101315, 101316, 120101, 120102, _
                                        120103, 120104, 120106, 120107, 120108, 121104, 121106, 140101, 140102, 140103, _
                                        140104, 140105, 140501, 140502, 140503, 140504, 140506, 140507, 140601, 140602, _
                                        140603, 140604, 140605, 140606, 140607, 140609, 140610, 140611, 140613, 140614, _
                                        140615, 140618, 140620, 140621, 140701, 140702, 140801, 140802, 140901, 141001, _
                                        141002, 141007, 141008, 141009, 141012, 141013, 141014, 141015, 141016, 204301, _
                                        204303, 204305, 204307, 204308, 204309, 204310, 204311, 204312, 204320, 204321, _
                                        204322, 20101, 20102, 20103, 20104, 20105, 211010)

    specialArticleGroupsOnlyViertelpalette = Array(60101, 60102, 60104, 60112, 60201, 60202, 60212, 60304, 60310, 60702, 60703)

    specialArticleGroupsOnlyHalbpalette = Array(100601, 100602, 100607, 100701, 100706, 100707, 100708, 100710, 100712, 100713)

    specialArticleGroupsSpMarkiesen = Array(140401, 140402, 140403, 140404, 140405, 140406, 140407, 140408, 140409, 140410, 140411)

    specialArticleGroupsSpHolz = Array(100101, 100102, 100103, 100104, 100106, 100107)

    specialArticleGroupsWandFliesen = Array(120101, 120102, 120103, 120106, 120107, 120108, 120401, 120402, 120403, 120404, _
                                            120405, 120406, 120407, 120701, 120702, 120703, 120704, 120705, 120706, 120707, _
                                            120708, 120709, 120710, 120711, 120712, 120714, 120715, 120716, 120717, 120718, _
                                            120719, 120720, 120722, 120723, 120726, 120727, 120728)

    specialArticleGroupsFliesen = Array(120201, 120203, 120206, 120207, 120208, 120301, 120303, 120305, 120306, 121104, 121106, _
                                        122201, 122202, 122302, 122304, 122306, 122308, 122309, 122310, 122311, 122312, 122401, 122402, 122403, 122406)

    specialArticleGroupsBandewannen = Array(131320, 131307, 131301, 131312, 131319, 131304, 131321, 131303, 131308, 131305)

    specialArticleGroupsSpiegel = Array(132501, 132502, 132503, 132504, 132505, 133301, 133302, 133303, 133390)

    specialBrandsSpTuere = Array("Jeld Wen", "Borne", "Marke Pertura")

    specialArticleGroupsSpTuere = Array(140601, 140602, 140603, 140604, 140605, 140606, 140611, 140613, _
                                        140614, 140615, 140618, 140620, 140621)

    specialArticleGroupsSpZargen = Array(140607, 140609, 140610)

    specialKnauf = Array("Knauf")

    specialGipsplatten = Array(20201, 20202, 20914)

    specialKühlschränke = Array(133503)

    lastRow = inputSheet.Cells(inputSheet.rows.Count, 1).End(xlUp).Row
    Debug.Print "Last row is: " & lastRow

    For i = 2 To lastRow
        versandweg = Trim(inputSheet.Cells(i, 9).Value)
        Debug.Print "Processing row " & i & " with Versandweg: '" & versandweg & "'"

        Select Case versandweg
            Case "MVZ"
                Call MVZ_VersandkostenBerechnen(i, specialBrands, specialMuster, specialArticleGroups400CHF, specialArticleGroups700CHF, specialArticleGroupsSackware, _
                                                specialArticleGroupsViertelpalette, specialArticleGroupsPalette, specialArticleGroupsOnlyViertelpalette, _
                                                specialArticleGroupsOnlyHalbpalette, specialArticleGroupsSpMarkiesen, specialArticleGroupsSpHolz, specialBrandsSpTuere, _
                                                specialArticleGroupsWandFliesen, specialArticleGroupsFliesen, specialArticleGroupsBandewannen, specialArticleGroupsSpiegel, specialArticleGroupsSpTuere, specialArticleGroupsSpZargen, specialKnauf, specialGipsplatten, specialKühlschränke)
            Case "FFDL"
                Call FFDL_VersandkostenBerechnen(i, specialBrands, specialMuster, specialArticleGroups400CHF, specialArticleGroups700CHF, specialArticleGroupsSackware, _
                                                 specialArticleGroupsViertelpalette, specialArticleGroupsPalette, specialArticleGroupsOnlyViertelpalette, _
                                                 specialArticleGroupsOnlyHalbpalette, specialArticleGroupsSpMarkiesen, specialArticleGroupsSpHolz, specialBrandsSpTuere, _
                                                 specialArticleGroupsWandFliesen, specialArticleGroupsFliesen, specialArticleGroupsBandewannen, specialArticleGroupsSpiegel, specialArticleGroupsSpTuere, specialArticleGroupsSpZargen, specialKnauf, specialGipsplatten, specialKühlschränke)
            Case "FFDL 484"
                Call FFDL484_VersandkostenBerechnen(i, specialBrands, specialMuster, specialArticleGroups50CHF, specialArticleGroups400CHF, specialArticleGroups700CHF, specialArticleGroupsSackware, _
                                                    specialArticleGroupsViertelpalette, specialArticleGroupsPalette, specialArticleGroupsOnlyViertelpalette, _
                                                    specialArticleGroupsOnlyHalbpalette, specialArticleGroupsSpMarkiesen, specialArticleGroupsSpHolz, specialBrandsSpTuere, _
                                                    specialArticleGroupsWandFliesen, specialArticleGroupsFliesen, specialArticleGroupsBandewannen, specialArticleGroupsSpiegel, specialArticleGroupsSpTuere, specialArticleGroupsSpZargen, specialKnauf, specialGipsplatten, specialKühlschränke)
            Case "VAL"
                Call VAL_VersandkostenBerechnen(i, specialBrands, specialMuster, specialArticleGroups400CHF, specialArticleGroups700CHF, specialArticleGroupsSackware, _
                                                specialArticleGroupsViertelpalette, specialArticleGroupsPalette, specialArticleGroupsOnlyViertelpalette, _
                                                specialArticleGroupsOnlyHalbpalette, specialArticleGroupsSpMarkiesen, specialArticleGroupsSpHolz, specialBrandsSpTuere, _
                                                specialArticleGroupsWandFliesen, specialArticleGroupsFliesen, specialArticleGroupsBandewannen, specialArticleGroupsSpiegel, specialArticleGroupsSpTuere, specialArticleGroupsSpZargen, specialKnauf, specialGipsplatten, specialKühlschränke)
            Case "VAL 3"
                Call VAL3_VersandkostenBerechnen(i, specialBrands, specialMuster, specialArticleGroups400CHF, specialArticleGroups700CHF, specialArticleGroupsSackware, _
                                                 specialArticleGroupsViertelpalette, specialArticleGroupsPalette, specialArticleGroupsOnlyViertelpalette, _
                                                 specialArticleGroupsOnlyHalbpalette, specialArticleGroupsSpMarkiesen, specialArticleGroupsSpHolz, specialBrandsSpTuere, _
                                                 specialArticleGroupsWandFliesen, specialArticleGroupsFliesen, specialArticleGroupsBandewannen, specialArticleGroupsSpiegel, specialArticleGroupsSpTuere, specialArticleGroupsSpZargen, specialKnauf, specialGipsplatten, specialKühlschränke)
            Case "Strecke"
                Call VAL_VersandkostenBerechnen(i, specialBrands, specialMuster, specialArticleGroups400CHF, specialArticleGroups700CHF, specialArticleGroupsSackware, _
                                                specialArticleGroupsViertelpalette, specialArticleGroupsPalette, specialArticleGroupsOnlyViertelpalette, _
                                                specialArticleGroupsOnlyHalbpalette, specialArticleGroupsSpMarkiesen, specialArticleGroupsSpHolz, specialBrandsSpTuere, _
                                                specialArticleGroupsWandFliesen, specialArticleGroupsFliesen, specialArticleGroupsBandewannen, specialArticleGroupsSpiegel, specialArticleGroupsSpTuere, specialArticleGroupsSpZargen, specialKnauf, specialGipsplatten, specialKühlschränke)
            Case Else
                ' Unbekannter Versandweg
                inputSheet.Cells(i, 10).Value = "Unbekannter Versandweg"
                inputSheet.Cells(i, 11).Value = "Unbekannter Versandweg"
        End Select
    Next i

    MsgBox "Zuordnung abgeschlossen!"
End Sub

Sub MVZ_VersandkostenBerechnen(ByVal rowIndex As Long, specialBrands As Variant, specialMuster As Variant, _
                               specialArticleGroups400CHF As Variant, specialArticleGroups700CHF As Variant, _
                               specialArticleGroupsSackware As Variant, specialArticleGroupsViertelpalette As Variant, _
                               specialArticleGroupsPalette As Variant, specialArticleGroupsOnlyViertelpalette As Variant, _
                               specialArticleGroupsOnlyHalbpalette As Variant, specialArticleGroupsSpMarkiesen As Variant, _
                               specialArticleGroupsSpHolz As Variant, specialBrandsSpTuere As Variant, _
                               specialArticleGroupsWandFliesen As Variant, specialArticleGroupsFliesen As Variant, _
                               specialArticleGroupsBandewannen As Variant, specialArticleGroupsSpiegel As Variant, _
                               specialArticleGroupsSpTuere As Variant, _
                               specialArticleGroupsSpZargen As Variant, specialKnauf As Variant, specialGipsplatten As Variant, _
                               specialKühlschränke As Variant)

    Dim inputSheet As Worksheet
    Dim artikel As String
    Dim artikelGruppe As Long
    Dim marke As String
    Dim länge As Double
    Dim breite As Double
    Dim tiefe As Double
    Dim gewicht As Double
    Dim versandkostenkennzeichen As Integer
    Dim versandart As Integer

    Set inputSheet = ThisWorkbook.Sheets("Aktuelle Artikeldaten")

    ' Grunddaten auslesen
    artikel = Trim(inputSheet.Cells(rowIndex, 1).Value)
    artikelGruppe = CLng(Trim(inputSheet.Cells(rowIndex, 3).Value))
    marke = Trim(inputSheet.Cells(rowIndex, 4).Value)
    länge = val(inputSheet.Cells(rowIndex, 5).Value)
    breite = val(inputSheet.Cells(rowIndex, 6).Value)
    tiefe = val(inputSheet.Cells(rowIndex, 7).Value)
    gewicht = val(inputSheet.Cells(rowIndex, 8).Value)
    
    versandkostenkennzeichen = 0
    versandart = 0

    If IsEmpty(artikel) Or artikel = "" Then
        inputSheet.Cells(rowIndex, 10).Value = "Kein Artikel"
        inputSheet.Cells(rowIndex, 11).Value = "Kein Artikel"
        Exit Sub
    End If

    Debug.Print "Starting checks for article goup: " & artikelGruppe

    ' SpecialBrands Regel
    Debug.Print "Checking spcialBrands..."
    Debug.Print "Artikelgruppe, die geprüft wird: " & artikelGruppe
    If IsInArray(marke, specialBrands) Then
        Debug.Print "Found in special Brands"
        längste_seite = Application.WorksheetFunction.Max(länge, breite, tiefe)
        gurtmass = 2 * (länge + breite + tiefe - längste_seite) + längste_seite
    
        If gewicht <= 30 And _
            ((länge <= 98 And breite <= 60 And tiefe <= 60) Or _
            (breite <= 98 And länge <= 60 And tiefe <= 60) Or _
            (tiefe <= 98 And länge <= 60 And breite <= 60)) Then
            versandkostenkennzeichen = 10
            versandart = 10
        ElseIf gewicht <= 10 And längste_seite <= 250 And gurtmass <= 400 Then
            versandkostenkennzeichen = 11
            versandart = 20
        ElseIf gewicht <= 30 And längste_seite <= 200 And gurtmass <= 400 Then
            versandkostenkennzeichen = 11
            versandart = 30
        Else
            versandkostenkennzeichen = 98
            versandart = 30
        End If
        
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    Debug.Print "Checking specialMuster...."
    Debug.Print "Artikelgruppe, die geprüft wird: " & artikelGruppe
    If IsInArray(artikelGruppe, specialMuster) Then
        Debug.Print "Found in special Muster"
        längste_seite = Application.WorksheetFunction.Max(länge, breite, tiefe)
        gurtmass = 2 * (länge + breite + tiefe - längste_seite) + längste_seite
        
        If gewicht <= 30 And _
            ((länge <= 98 And breite <= 60 And tiefe <= 60) Or _
            (breite <= 98 And länge <= 60 And tiefe <= 60) Or _
            (tiefe <= 98 And länge <= 60 And breite <= 60)) Then
            versandkostenkennzeichen = 98
            versandart = 10
        ElseIf gewicht <= 10 And längste_seite <= 250 And gurtmass <= 400 Then
            versandkostenkennzeichen = 98
            versandart = 20
        ElseIf gewicht <= 30 And längste_seite <= 200 And gurtmass <= 400 Then
            versandkostenkennzeichen = 98
            versandart = 20
        Else
            versandkostenkennzeichen = 98
            versandart = 30
        End If
        
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    Debug.Print "Checking SpTuere..."
    Debug.Print "Artikelgruppe, die geprüft wird: " & artikelGruppe
    If IsInArray(artikelGruppe, specialArticleGroupsSpTuere) And IsInArray(marke, specialBrandsSpTuere) Then
        Debug.Print "Found in SpTuere"
        versandkostenkennzeichen = 36
        versandart = 30
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    Debug.Print "Chechking SpZargen..."
    Debug.Print "Artikelgruppe, die geprüft wird: " & artikelGruppe
    If IsInArray(artikelGruppe, specialArticleGroupsSpZargen) And IsInArray(marke, specialBrandsSpTuere) Then
        Debug.Print "Found in SpZargen"
        versandkostenkennzeichen = 30
        versandart = 30
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    Debug.Print "Checking Gipsplatten..."
    Debug.Print "Artikelgruppe, die geprüft wird: " & artikelGruppe
    If IsInArray(artikelGruppe, specialGipsplatten) And IsInArray(marke, specialKnauf) Then
        Debug.Print "Found in Gipsplatten"
        versandkostenkennzeichen = 49
        versandart = 30
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    Debug.Print "Checking OnlyViertelpalette"
    Debug.Print "Artikelgruppe, die geprüft wird: " & artikelGruppe
    If IsInArray(artikelGruppe, specialArticleGroupsOnlyViertelpalette) Then
        Debug.Print "Found in OnlyViertelpalette"
        versandkostenkennzeichen = 31
        versandart = 30
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    Debug.Print "Checking OnlyHalbpalette"
    Debug.Print "Artikelgruppe, die geprüft wird: " & artikelGruppe
    If IsInArray(artikelGruppe, specialArticleGroupsOnlyHalbpalette) Then
        Debug.Print "Found in OnlyHalbpalette"
        versandkostenkennzeichen = 32
        versandart = 30
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    ' Prüfung auf spezielle Artikelgruppen WandFliesen
    Debug.Print "Checking WandFliesen"
    Debug.Print "Artikelgruppe, die geprüft wird: " & artikelGruppe
    If IsInArray(artikelGruppe, specialArticleGroupsWandFliesen) Then
        Debug.Print "Special Article Group Wandfliesen found (MVZ): " & artikelGruppe
        Call MVZ_BestimmeVersandkostenWandFliesen(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        Debug.Print "After WandFliesen: VKK=" & versandkostenkennzeichen & ", VA=" & versandart
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    ' Prüfung auf spezielle Artikelgruppen mit 400 CHF
    Debug.Print "Checking 400CHF"
    Debug.Print "Artikelgruppe, die geprüft wird: " & artikelGruppe
    If IsInArray(artikelGruppe, specialArticleGroups400CHF) Then
        Debug.Print "Special Article Group 400 CHF found (MVZ): " & artikelGruppe
        Call MVZ_BestimmeVersandkostenSpezial400CHF(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    ' Prüfung auf spezielle Artikelgruppen mit 700 CHF
    Debug.Print "Checking 700 CHF"
    Debug.Print "Artikelgruppe, die geprüft wird: " & artikelGruppe
    If IsInArray(artikelGruppe, specialArticleGroups700CHF) Then
        Debug.Print "Special Article Group 700 CHF found (MVZ): " & artikelGruppe
        Call MVZ_BestimmeVersandkostenSpezial700CHF(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    ' Prüfung auf Sackware
    Debug.Print "Checking Sackware"
    Debug.Print "Artikelgruppe, die geprüft wird: " & artikelGruppe
    If IsInArray(artikelGruppe, specialArticleGroupsSackware) Then
        Debug.Print "Special Article Group Sackware found (MVZ): " & artikelGruppe
        Call MVZ_BestimmeVersandkostenSpezialSackware(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    'Prüfung auf spezielle Artikelgruppen minimum Viertelpalette
    Debug.Print "Checking Viertelpalette"
    Debug.Print "Artikelgruppe, die geprüft wird: " & artikelGruppe
    If IsInArray(artikelGruppe, specialArticleGroupsViertelpalette) Then
        Debug.Print "Special Article Group Viertelpalette found (MVZ): " & artikelGruppe
        Call MVZ_BestimmeVersandkostenSpezialViertelpalette(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    'Prüfung auf spezielle Artikelgruppen minimum Palette
    Debug.Print "Checking Palette"
    Debug.Print "Artikelgruppe, die geprüft wird: " & artikelGruppe
    If IsInArray(artikelGruppe, specialArticleGroupsPalette) Then
        Debug.Print "Special Article Group Palette found (MVZ): " & artikelGruppe
        Call MVZ_BestimmeVersandkostenSpezialPalette(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    'Prüfung auf spezielle Artikelgruppen Sp Markiesen
    Debug.Print "Checking Sp Markiesen"
    Debug.Print "Artikelgruppe, die geprüft wird: " & artikelGruppe
    If IsInArray(artikelGruppe, specialArticleGroupsSpMarkiesen) Then
        Debug.Print "Special Article Group Sp Markiesen found (MVZ): " & artikelGruppe
        Call MVZ_BestimmeVersandkostenSpMarkiesen(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    'Prüfung auf spezielle Artikelgruppe Sp Holz
    Debug.Print "Checking Sp Holz"
    Debug.Print "Artikelgruppe, die geprüft wird: " & artikelGruppe
    If IsInArray(artikelGruppe, specialArticleGroupsSpHolz) Then
        Debug.Print "Special Article Group Sp Holz found (MVZ): " & artikelGruppe
        Call MVZ_BestimmeVersandkostenSpHolz(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    'Prüfung auf spezielle Artikelgruppe Fliesen
    Debug.Print "Checking Fliesen"
    Debug.Print "Artikelgruppe, die geprüft wird: " & artikelGruppe
    If IsInArray(artikelGruppe, specialArticleGroupsFliesen) Then
        Debug.Print "Special Article Group Fliesen found (MVZ): " & artikelGruppe
        Call MVZ_BestimmeVersandkostenFliesen(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    'Prüfung auf spezielle Artikelgruppe Badewannen
    Debug.Print "Checking Badewannen"
    Debug.Print "Artikelgruppe, die geprüft wird: " & artikelGruppe
    If IsInArray(artikelGruppe, specialArticleGroupsBandewannen) Then
        Debug.Print "Special Article Group Badewannen found (MVZ): " & artikelGruppe
        Call MVZ_BestimmeVersandkostenBadewannen(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    'Prüfung auf spezielle Artikelgruppe Spiegel
    Debug.Print "Checking Spiegel"
    Debug.Print "Artikelgruppe, die geprüft wird: " & artikelGruppe
    If IsInArray(artikelGruppe, specialArticleGroupsSpiegel) Then
        Debug.Print "Special Article Group Spiegel found (MVZ): " & artikelGruppe
        Call MVZ_BestimmeVersandkostenSpiegel(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    'Prüfung auf spezielle Artikelgruppe Kühlschränke
    Debug.Print "Checking Kühlschränke"
    Debug.Print "Artikelgruppe, die geprüft wird: " & artikelGruppe
    If IsInArray(artikelGruppe, specialKühlschränke) Then
        Debug.Print "Special Article Group Kühlschränke found (MVZ): " & artikelGruppe
        Call MVZ_BestimmeVersandkostenKühlschränke(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    ' Standard-Versandkostenlogik
    Debug.Print "Normal shipping logic applied. (MVZ)"
    Debug.Print "Artikelgruppe, die geprüft wird: " & artikelGruppe
    Call MVZ_BestimmeVersandkosten(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
    inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
    inputSheet.Cells(rowIndex, 11).Value = versandart
End Sub

Sub FFDL_VersandkostenBerechnen(ByVal rowIndex As Long, specialBrands As Variant, specialMuster As Variant, _
                                specialArticleGroups400CHF As Variant, specialArticleGroups700CHF As Variant, _
                                specialArticleGroupsSackware As Variant, specialArticleGroupsViertelpalette As Variant, _
                                specialArticleGroupsPalette As Variant, specialArticleGroupsOnlyViertelpalette As Variant, _
                                specialArticleGroupsOnlyHalbpalette As Variant, specialArticleGroupsSpMarkiesen As Variant, _
                                specialArticleGroupsSpHolz As Variant, specialBrandsSpTuere As Variant, _
                                specialArticleGroupsWandFliesen As Variant, specialArticleGroupsFliesen As Variant, _
                                specialArticleGroupsBandewannen As Variant, specialArticleGroupsSpiegel As Variant, _
                                specialArticleGroupsSpTuere As Variant, _
                                specialArticleGroupsSpZargen As Variant, specialKnauf As Variant, specialGipsplatten As Variant, _
                                specialKühlschränke As Variant)

    Dim inputSheet As Worksheet
    Dim artikel As String
    Dim artikelGruppe As Long
    Dim marke As String
    Dim länge As Double
    Dim breite As Double
    Dim tiefe As Double
    Dim gewicht As Double
    Dim versandkostenkennzeichen As Integer
    Dim versandart As Integer

    Set inputSheet = ThisWorkbook.Sheets("Aktuelle Artikeldaten")

    artikel = Trim(inputSheet.Cells(rowIndex, 1).Value)
    artikelGruppe = CLng(Trim(inputSheet.Cells(rowIndex, 3).Value))
    marke = Trim(inputSheet.Cells(rowIndex, 4).Value)
    
    versandkostenkennzeichen = 0
    versandart = 0

    If IsEmpty(artikel) Or artikel = "" Then
        inputSheet.Cells(rowIndex, 10).Value = "Kein Artikel"
        inputSheet.Cells(rowIndex, 11).Value = "Kein Artikel"
        Exit Sub
    End If

    länge = val(inputSheet.Cells(rowIndex, 5).Value)
    breite = val(inputSheet.Cells(rowIndex, 6).Value)
    tiefe = val(inputSheet.Cells(rowIndex, 7).Value)
    gewicht = val(inputSheet.Cells(rowIndex, 8).Value)

    ' Spezielle Marken und Kombinationen zuerst prüfen
    If IsInArray(marke, specialBrands) Then
        längste_seite = Application.WorksheetFunction.Max(länge, breite, tiefe)
        gurtmass = 2 * (länge + breite + tiefe - längste_seite) + längste_seite
    
        ' Regel 1: Spezialmarken mit Maßbeschränkungen
        If gewicht <= 30 And _
            ((länge <= 98 And breite <= 60 And tiefe <= 60) Or _
            (breite <= 98 And länge <= 60 And tiefe <= 60) Or _
            (tiefe <= 98 And länge <= 60 And breite <= 60)) Then
        
            versandkostenkennzeichen = 58
            versandart = 10
    
        ' Regel 2: Standardversand mit Größenbeschränkungen
        ElseIf gewicht <= 10 And längste_seite <= 250 And gurtmass <= 400 Then
            versandkostenkennzeichen = 57
            versandart = 20

        ' Dritte Regel: Max 30kg, längste Seite max 200cm, Gurtmaß max 400cm
        ElseIf gewicht <= 30 And längste_seite <= 200 And gurtmass <= 400 Then
            versandkostenkennzeichen = 57
            versandart = 30
    
        ' Wenn keine der obigen Regeln zutrifft
        Else
            versandkostenkennzeichen = 35
            versandart = 30
        End If

        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    Debug.Print "Checking specialMuster...."
    Debug.Print "Artikelgruppe, die geprüft wird: " & artikelGruppe
    If IsInArray(artikelGruppe, specialMuster) Then
        Debug.Print "Found in special Muster"
        längste_seite = Application.WorksheetFunction.Max(länge, breite, tiefe)
        gurtmass = 2 * (länge + breite + tiefe - längste_seite) + längste_seite
        
        If gewicht <= 30 And _
            ((länge <= 98 And breite <= 60 And tiefe <= 60) Or _
            (breite <= 98 And länge <= 60 And tiefe <= 60) Or _
            (tiefe <= 98 And länge <= 60 And breite <= 60)) Then
            versandkostenkennzeichen = 35
            versandart = 10
        ElseIf gewicht <= 10 And längste_seite <= 250 And gurtmass <= 400 Then
            versandkostenkennzeichen = 35
            versandart = 20
        ElseIf gewicht <= 30 And längste_seite <= 200 And gurtmass <= 400 Then
            versandkostenkennzeichen = 35
            versandart = 20
        Else
            versandkostenkennzeichen = 35
            versandart = 30
        End If
        
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    ' Spezielle Kombinationen von Artikelgruppen und Marken
    If IsInArray(artikelGruppe, specialArticleGroupsSpTuere) And IsInArray(marke, specialBrandsSpTuere) Then
        versandkostenkennzeichen = 36
        versandart = 30
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    If IsInArray(artikelGruppe, specialArticleGroupsSpZargen) And IsInArray(marke, specialBrandsSpTuere) Then
        versandkostenkennzeichen = 56
        versandart = 30
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    If IsInArray(artikelGruppe, specialGipsplatten) And IsInArray(marke, specialKnauf) Then
        versandkostenkennzeichen = 49
        versandart = 30
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    ' Spezielle Palette-Typen
    If IsInArray(artikelGruppe, specialArticleGroupsOnlyViertelpalette) Then
        versandkostenkennzeichen = 55
        versandart = 30
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    If IsInArray(artikelGruppe, specialArticleGroupsOnlyHalbpalette) Then
        versandkostenkennzeichen = 0
        versandart = 0
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    ' Prüfung auf spezielle Artikelgruppen
    If IsInArray(artikelGruppe, specialArticleGroupsWandFliesen) Then
        Debug.Print "Special Article Group Wandfliesen found (FFDL): " & artikelGruppe
        Call FFDL_BestimmeVersandkostenWandFliesen(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    If IsInArray(artikelGruppe, specialArticleGroups400CHF) Then
        Debug.Print "Special Article Group 400 CHF found (FFDL): " & artikelGruppe
        Call FFDL_BestimmeVersandkostenSpezial400CHF(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    If IsInArray(artikelGruppe, specialArticleGroups700CHF) Then
        Debug.Print "Special Article Group 700 CHF found (FFDL): " & artikelGruppe
        Call FFDL_BestimmeVersandkostenSpezial700CHF(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    If IsInArray(artikelGruppe, specialArticleGroupsSackware) Then
        Debug.Print "Special Article Group Sackware found (FFDL): " & artikelGruppe
        Call FFDL_BestimmeVersandkostenSpezialSackware(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    If IsInArray(artikelGruppe, specialArticleGroupsViertelpalette) Then
        Debug.Print "Special Article Group Viertelpalette found (FFDL): " & artikelGruppe
        Call FFDL_BestimmeVersandkostenSpezialViertelpalette(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    If IsInArray(artikelGruppe, specialArticleGroupsPalette) Then
        Debug.Print "Special Article Group Palette found (FFDL): " & artikelGruppe
        Call FFDL_BestimmeVersandkostenSpezialPalette(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    If IsInArray(artikelGruppe, specialArticleGroupsSpMarkiesen) Then
        Debug.Print "Special Article Group Sp Markiesen found (FFDL): " & artikelGruppe
        Call FFDL_BestimmeVersandkostenSpMarkiesen(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    If IsInArray(artikelGruppe, specialArticleGroupsSpHolz) Then
        Debug.Print "Special Article Group Sp Holz found (FFDL): " & artikelGruppe
        Call FFDL_BestimmeVersandkostenSpHolz(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    If IsInArray(artikelGruppe, specialArticleGroupsFliesen) Then
        Debug.Print "Special Article Group Fliesen found (FFDL): " & artikelGruppe
        Call FFDL_BestimmeVersandkostenFliesen(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    If IsInArray(artikelGruppe, specialArticleGroupsBandewannen) Then
        Debug.Print "Special Article Group Badewannen found (FFDL): " & artikelGruppe
        Call FFDL_BestimmeVersandkostenBandewannen(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    If IsInArray(artikelGruppe, specialArticleGroupsSpiegel) Then
        Debug.Print "Special Article Group Spiegel found (FFDL): " & artikelGruppe
        Call FFDL_BestimmeVersandkostenSpiegel(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    If IsInArray(artikelGruppe, specialKühlschränke) Then
        Debug.Print "Special Article Group Kühlschränke found (FFDL): " & artikelGruppe
        Call FFDL_BestimmeVersandkostenKühlschränke(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    ' Standard-Versandkostenlogik als Fallback
    Debug.Print "Normal shipping logic applied. (FFDL)"
    Call FFDL_BestimmeVersandkosten(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
    inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
    inputSheet.Cells(rowIndex, 11).Value = versandart
End Sub

Sub FFDL484_VersandkostenBerechnen(ByVal rowIndex As Long, specialBrands As Variant, specialMuster As Variant, _
                                   specialArticleGroups50CHF As Variant, specialArticleGroups400CHF As Variant, _
                                   specialArticleGroups700CHF As Variant, specialArticleGroupsSackware As Variant, _
                                   specialArticleGroupsViertelpalette As Variant, specialArticleGroupsPalette As Variant, _
                                   specialArticleGroupsOnlyViertelpalette As Variant, specialArticleGroupsOnlyHalbpalette As Variant, _
                                   specialArticleGroupsSpMarkiesen As Variant, specialArticleGroupsSpHolz As Variant, _
                                   specialBrandsSpTuere As Variant, specialArticleGroupsWandFliesen As Variant, _
                                   specialArticleGroupsFliesen As Variant, specialArticleGroupsBandewannen As Variant, _
                                   specialArticleGroupsSpiegel As Variant, _
                                   specialArticleGroupsSpTuere As Variant, specialArticleGroupsSpZargen As Variant, _
                                   specialKnauf As Variant, specialGipsplatten As Variant, specialKühlschränke As Variant)

    Dim inputSheet As Worksheet
    Dim artikel As String
    Dim artikelGruppe As Long
    Dim marke As String
    Dim länge As Double
    Dim breite As Double
    Dim tiefe As Double
    Dim gewicht As Double
    Dim versandkostenkennzeichen As Integer
    Dim versandart As Integer

    Set inputSheet = ThisWorkbook.Sheets("Aktuelle Artikeldaten")

    ' Grunddaten auslesen
    artikel = Trim(inputSheet.Cells(rowIndex, 1).Value)
    artikelGruppe = CLng(Trim(inputSheet.Cells(rowIndex, 3).Value))
    marke = Trim(inputSheet.Cells(rowIndex, 4).Value)
    länge = val(inputSheet.Cells(rowIndex, 5).Value)
    breite = val(inputSheet.Cells(rowIndex, 6).Value)
    tiefe = val(inputSheet.Cells(rowIndex, 7).Value)
    gewicht = val(inputSheet.Cells(rowIndex, 8).Value)
    
    versandkostenkennzeichen = 0
    versandart = 0

    If IsEmpty(artikel) Or artikel = "" Then
        inputSheet.Cells(rowIndex, 10).Value = "Kein Artikel"
        inputSheet.Cells(rowIndex, 11).Value = "Kein Artikel"
        Exit Sub
    End If

    ' SpecialBrands Regel
    If IsInArray(marke, specialBrands) Then
        längste_seite = Application.WorksheetFunction.Max(länge, breite, tiefe)
        gurtmass = 2 * (länge + breite + tiefe - längste_seite) + längste_seite
    
        If gewicht <= 30 And _
            ((länge <= 98 And breite <= 60 And tiefe <= 60) Or _
            (breite <= 98 And länge <= 60 And tiefe <= 60) Or _
            (tiefe <= 98 And länge <= 60 And breite <= 60)) Then
            versandkostenkennzeichen = 14
            versandart = 10
        ElseIf gewicht <= 10 And längste_seite <= 250 And gurtmass <= 400 Then
            versandkostenkennzeichen = 15
            versandart = 20
        ElseIf gewicht <= 30 And längste_seite <= 200 And gurtmass <= 400 Then
            versandkostenkennzeichen = 15
            versandart = 30
        Else
            versandkostenkennzeichen = 35
            versandart = 30
        End If
        
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    Debug.Print "Checking specialMuster...."
    Debug.Print "Artikelgruppe, die geprüft wird: " & artikelGruppe
    If IsInArray(artikelGruppe, specialMuster) Then
        Debug.Print "Found in special Muster"
        längste_seite = Application.WorksheetFunction.Max(länge, breite, tiefe)
        gurtmass = 2 * (länge + breite + tiefe - längste_seite) + längste_seite
        
        If gewicht <= 30 And _
            ((länge <= 98 And breite <= 60 And tiefe <= 60) Or _
            (breite <= 98 And länge <= 60 And tiefe <= 60) Or _
            (tiefe <= 98 And länge <= 60 And breite <= 60)) Then
            versandkostenkennzeichen = 35
            versandart = 10
        ElseIf gewicht <= 10 And längste_seite <= 250 And gurtmass <= 400 Then
            versandkostenkennzeichen = 35
            versandart = 20
        ElseIf gewicht <= 30 And längste_seite <= 200 And gurtmass <= 400 Then
            versandkostenkennzeichen = 35
            versandart = 20
        Else
            versandkostenkennzeichen = 35
            versandart = 30
        End If
        
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    If IsInArray(artikelGruppe, specialArticleGroupsSpTuere) And IsInArray(marke, specialBrandsSpTuere) Then
        versandkostenkennzeichen = 36
        versandart = 30
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    If IsInArray(artikelGruppe, specialArticleGroupsSpZargen) And IsInArray(marke, specialBrandsSpTuere) Then
        versandkostenkennzeichen = 18
        versandart = 30
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    If IsInArray(artikelGruppe, specialGipsplatten) And IsInArray(marke, specialKnauf) Then
        versandkostenkennzeichen = 49
        versandart = 30
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    If IsInArray(artikelGruppe, specialArticleGroupsOnlyViertelpalette) Then
        versandkostenkennzeichen = 16
        versandart = 30
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    If IsInArray(artikelGruppe, specialArticleGroupsOnlyHalbpalette) Then
        versandkostenkennzeichen = 17
        versandart = 30
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    ' Prüfung auf spezielle Artikel mit 50 CHF
    If IsInArray(artikelGruppe, specialArticleGroups50CHF) Then
        Debug.Print "Special Article Group 50 CHF found (FFDL484): " & artikelGruppe
        Call FFDL484_BestimmeVersandkostenSpezial50CHF(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    ' Prüfung auf spezielle Artikelgruppen WandFliesen
    If IsInArray(artikelGruppe, specialArticleGroupsWandFliesen) Then
        Debug.Print "Special Article Group Wandfliesen found (FFDL484): " & artikelGruppe
        Call FFDL484_BestimmeVersandkostenWandfliesen(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    ' Prüfung auf spezielle Artikelgruppen mit 400 CHF
    If IsInArray(artikelGruppe, specialArticleGroups400CHF) Then
        Debug.Print "Special Article Group 400 CHF found (FFDL484): " & artikelGruppe
        Call FFDL484_BestimmeVersandkostenSpezial400CHF(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    ' Prüfung auf spezielle Artikelgruppen mit 700 CHF
    If IsInArray(artikelGruppe, specialArticleGroups700CHF) Then
        Debug.Print "Special Article Group 700 CHF found (FFDL484): " & artikelGruppe
        Call FFDL484_BestimmeVersandkostenSpezial700CHF(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    ' Prüfung auf Sackware
    If IsInArray(artikelGruppe, specialArticleGroupsSackware) Then
        Debug.Print "Special Article Group Sackware found (FFDL484): " & artikelGruppe
        Call FFDL484_BestimmeVersandkostenSpezialSackware(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    'Prüfung auf spezielle Artikelgruppen minimum Viertelpalette
    If IsInArray(artikelGruppe, specialArticleGroupsViertelpalette) Then
        Debug.Print "Special Article Group Viertelpalette found (FFDL484): " & artikelGruppe
        Call FFDL484_BestimmeVersandkostenSpezialViertelpalette(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    'Prüfung auf spezielle Artikelgruppen minimum Palette
    If IsInArray(artikelGruppe, specialArticleGroupsPalette) Then
        Debug.Print "Special Article Group Palette found (FFDL484): " & artikelGruppe
        Call FFDL484_BestimmeVersandkostenSpezialPalette(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    'Prüfung auf spezielle Artikelgruppen Sp Markiesen
    If IsInArray(artikelGruppe, specialArticleGroupsSpMarkiesen) Then
        Debug.Print "Special Article Group Sp Markiesen found (FFDL484): " & artikelGruppe
        Call FFDL484_BestimmeVersandkostenSpMarkiesen(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    'Prüfung auf spezielle Artikelgruppe Sp Holz
    If IsInArray(artikelGruppe, specialArticleGroupsSpHolz) Then
        Debug.Print "Special Article Group Sp Holz found (FFDL484): " & artikelGruppe
        Call FFDL484_BestimmeVersandkostenSpHolz(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    'Prüfung auf spezielle Artikelgruppe Fliesen
    If IsInArray(artikelGruppe, specialArticleGroupsFliesen) Then
        Debug.Print "Special Article Group Fliesen found (FFDL484): " & artikelGruppe
        Call FFDL484_BestimmeVersandkostenFliesen(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    'Prüfung auf spezielle Artikelgruppe Badewannen
    If IsInArray(artikelGruppe, specialArticleGroupsBandewannen) Then
        Debug.Print "Special Article Group Badewannen found (FFDL484): " & artikelGruppe
        Call FFDL484_BestimmeVersandkostenBadewannen(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    'Prüfung auf spezielle Artikelgruppe Spiegel
    If IsInArray(artikelGruppe, specialArticleGroupsSpiegel) Then
        Debug.Print "Special Article Group Spiegel found (FFDL484): " & artikelGruppe
        Call FFDL484_BestimmeVersandkostenSpiegel(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    'Prüfung auf spezielle Artikelgruppe Kühlschränke
    If IsInArray(artikelGruppe, specialKühlschränke) Then
        Debug.Print "Special Article Group Kühlschränke found (FFDL484): " & artikelGruppe
        Call FFDL484_BestimmeVersandkostenKühlschränke(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    ' Standard-Versandkostenlogik
    Debug.Print "Normal shipping logic applied. (FFDL484)"
    Call FFDL484_BestimmeVersandkosten(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
    inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
    inputSheet.Cells(rowIndex, 11).Value = versandart
End Sub

Sub VAL_VersandkostenBerechnen(ByVal rowIndex As Long, specialBrands As Variant, specialMuster As Variant, _
                               specialArticleGroups400CHF As Variant, specialArticleGroups700CHF As Variant, _
                               specialArticleGroupsSackware As Variant, specialArticleGroupsViertelpalette As Variant, _
                               specialArticleGroupsPalette As Variant, specialArticleGroupsOnlyViertelpalette As Variant, _
                               specialArticleGroupsOnlyHalbpalette As Variant, specialArticleGroupsSpMarkiesen As Variant, _
                               specialArticleGroupsSpHolz As Variant, specialBrandsSpTuere As Variant, _
                               specialArticleGroupsWandFliesen As Variant, specialArticleGroupsFliesen As Variant, _
                               specialArticleGroupsBandewannen As Variant, specialArticleGroupsSpiegel As Variant, _
                               specialArticleGroupsSpTuere As Variant, _
                               specialArticleGroupsSpZargen As Variant, specialKnauf As Variant, specialGipsplatten As Variant, _
                               specialKühlschränke As Variant)

    Dim inputSheet As Worksheet
    Dim artikel As String
    Dim artikelGruppe As Long
    Dim marke As String
    Dim länge As Double
    Dim breite As Double
    Dim tiefe As Double
    Dim gewicht As Double
    Dim versandkostenkennzeichen As Integer
    Dim versandart As Integer

    Set inputSheet = ThisWorkbook.Sheets("Aktuelle Artikeldaten")

    ' Grunddaten auslesen
    artikel = Trim(inputSheet.Cells(rowIndex, 1).Value)
    artikelGruppe = CLng(Trim(inputSheet.Cells(rowIndex, 3).Value))
    marke = Trim(inputSheet.Cells(rowIndex, 4).Value)
    länge = val(inputSheet.Cells(rowIndex, 5).Value)
    breite = val(inputSheet.Cells(rowIndex, 6).Value)
    tiefe = val(inputSheet.Cells(rowIndex, 7).Value)
    gewicht = val(inputSheet.Cells(rowIndex, 8).Value)
    
    versandkostenkennzeichen = 0
    versandart = 0

    If IsEmpty(artikel) Or artikel = "" Then
        inputSheet.Cells(rowIndex, 10).Value = "Kein Artikel"
        inputSheet.Cells(rowIndex, 11).Value = "Kein Artikel"
        Exit Sub
    End If

    ' SpecialBrands Regel
    If IsInArray(marke, specialBrands) Then
        längste_seite = Application.WorksheetFunction.Max(länge, breite, tiefe)
        gurtmass = 2 * (länge + breite + tiefe - längste_seite) + längste_seite
    
        If gewicht <= 30 And _
            ((länge <= 98 And breite <= 60 And tiefe <= 60) Or _
            (breite <= 98 And länge <= 60 And tiefe <= 60) Or _
            (tiefe <= 98 And länge <= 60 And breite <= 60)) Then
            versandkostenkennzeichen = 51
            versandart = 10
        ElseIf gewicht <= 10 And längste_seite <= 250 And gurtmass <= 400 Then
            versandkostenkennzeichen = 52
            versandart = 20
        ElseIf gewicht <= 30 And längste_seite <= 200 And gurtmass <= 400 Then
            versandkostenkennzeichen = 52
            versandart = 30
        Else
            versandkostenkennzeichen = 98
            versandart = 30
        End If
        
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    Debug.Print "Checking specialMuster...."
    Debug.Print "Artikelgruppe, die geprüft wird: " & artikelGruppe
    If IsInArray(artikelGruppe, specialMuster) Then
        Debug.Print "Found in special Muster"
        längste_seite = Application.WorksheetFunction.Max(länge, breite, tiefe)
        gurtmass = 2 * (länge + breite + tiefe - längste_seite) + längste_seite
        
        If gewicht <= 30 And _
            ((länge <= 98 And breite <= 60 And tiefe <= 60) Or _
            (breite <= 98 And länge <= 60 And tiefe <= 60) Or _
            (tiefe <= 98 And länge <= 60 And breite <= 60)) Then
            versandkostenkennzeichen = 98
            versandart = 10
        ElseIf gewicht <= 10 And längste_seite <= 250 And gurtmass <= 400 Then
            versandkostenkennzeichen = 98
            versandart = 20
        ElseIf gewicht <= 30 And längste_seite <= 200 And gurtmass <= 400 Then
            versandkostenkennzeichen = 98
            versandart = 20
        Else
            versandkostenkennzeichen = 98
            versandart = 30
        End If
        
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    If IsInArray(artikelGruppe, specialArticleGroupsSpTuere) And IsInArray(marke, specialBrandsSpTuere) Then
        versandkostenkennzeichen = 36
        versandart = 30
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    If IsInArray(artikelGruppe, specialArticleGroupsSpZargen) And IsInArray(marke, specialBrandsSpTuere) Then
        versandkostenkennzeichen = 53
        versandart = 30
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    If IsInArray(artikelGruppe, specialGipsplatten) And IsInArray(marke, specialKnauf) Then
        versandkostenkennzeichen = 49
        versandart = 30
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    If IsInArray(artikelGruppe, specialArticleGroupsOnlyViertelpalette) Then
        versandkostenkennzeichen = 31
        versandart = 30
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    If IsInArray(artikelGruppe, specialArticleGroupsOnlyHalbpalette) Then
        versandkostenkennzeichen = 32
        versandart = 30
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    ' Prüfung auf spezielle Artikelgruppen WandFliesen
    If IsInArray(artikelGruppe, specialArticleGroupsWandFliesen) Then
        Debug.Print "Special Article Group Wandfliesen found (VAL): " & artikelGruppe
        Call VAL_BestimmeVersandkostenWandFliesen(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    ' Prüfung auf spezielle Artikelgruppen mit 400 CHF
    If IsInArray(artikelGruppe, specialArticleGroups400CHF) Then
        Debug.Print "Special Article Group 400 CHF found (VAL): " & artikelGruppe
        Call VAL_BestimmeVersandkostenSpezial400CHF(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    ' Prüfung auf spezielle Artikelgruppen mit 700 CHF
    If IsInArray(artikelGruppe, specialArticleGroups700CHF) Then
        Debug.Print "Special Article Group 700 CHF found (VAL): " & artikelGruppe
        Call VAL_BestimmeVersandkostenSpezial700CHF(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    ' Prüfung auf Sackware
    If IsInArray(artikelGruppe, specialArticleGroupsSackware) Then
        Debug.Print "Special Article Group Sackware found (VAL): " & artikelGruppe
        Call VAL_BestimmeVersandkostenSpezialSackware(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    'Prüfung auf spezielle Artikelgruppen minimum Viertelpalette
    If IsInArray(artikelGruppe, specialArticleGroupsViertelpalette) Then
        Debug.Print "Special Article Group Viertelpalette found (VAL): " & artikelGruppe
        Call VAL_BestimmeVersandkostenSpezialViertelpalette(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    'Prüfung auf spezielle Artikelgruppen minimum Palette
    If IsInArray(artikelGruppe, specialArticleGroupsPalette) Then
        Debug.Print "Special Article Group Palette found (VAL): " & artikelGruppe
        Call VAL_BestimmeVersandkostenSpezialPalette(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    'Prüfung auf spezielle Artikelgruppen Sp Markiesen
    If IsInArray(artikelGruppe, specialArticleGroupsSpMarkiesen) Then
        Debug.Print "Special Article Group Sp Markiesen found (VAL): " & artikelGruppe
        Call VAL_BestimmeVersandkostenSpMarkiesen(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    'Prüfung auf spezielle Artikelgruppe Sp Holz
    If IsInArray(artikelGruppe, specialArticleGroupsSpHolz) Then
        Debug.Print "Special Article Group Sp Holz found (VAL): " & artikelGruppe
        Call VAL_BestimmeVersandkostenSpHolz(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    'Prüfung auf spezielle Artikelgruppe Fliesen
    If IsInArray(artikelGruppe, specialArticleGroupsFliesen) Then
        Debug.Print "Special Article Group Fliesen found (VAL): " & artikelGruppe
        Call VAL_BestimmeVersandkostenFliesen(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    'Prüfung auf spezielle Artikelgruppe Badewannen
    If IsInArray(artikelGruppe, specialArticleGroupsBandewannen) Then
        Debug.Print "Special Article Group Badewannen found (VAL): " & artikelGruppe
        Call VAL_BestimmeVersandkostenBadewannen(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    'Prüfung auf spezielle Artikelgruppe Spiegel
    If IsInArray(artikelGruppe, specialArticleGroupsSpiegel) Then
        Debug.Print "Special Article Group Spiegel found (VAL): " & artikelGruppe
        Call VAL_BestimmeVersandkostenSpiegel(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    'PRüfung auf spezielle Artikelgruppe Kühlschränke
    If IsInArray(artikelGruppe, specialKühlschränke) Then
        Debug.Print "Special Article Group Kühlschränke found (VAL): " & artikelGruppe
        Call VAL_BestimmeVersandkostenKühlschränke(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    ' Standard-Versandkostenlogik
    Debug.Print "Normal shipping logic applied. (VAL)"
    Call VAL_BestimmeVersandkosten(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
    inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
    inputSheet.Cells(rowIndex, 11).Value = versandart
End Sub

Sub VAL3_VersandkostenBerechnen(ByVal rowIndex As Long, specialBrands As Variant, specialMuster As Variant, _
                                specialArticleGroups400CHF As Variant, specialArticleGroups700CHF As Variant, _
                                specialArticleGroupsSackware As Variant, specialArticleGroupsViertelpalette As Variant, _
                                specialArticleGroupsPalette As Variant, specialArticleGroupsOnlyViertelpalette As Variant, _
                                specialArticleGroupsOnlyHalbpalette As Variant, specialArticleGroupsSpMarkiesen As Variant, _
                                specialArticleGroupsSpHolz As Variant, specialBrandsSpTuere As Variant, _
                                specialArticleGroupsWandFliesen As Variant, specialArticleGroupsFliesen As Variant, _
                                specialArticleGroupsBandewannen As Variant, specialArticleGroupsSpiegel As Variant, _
                                specialArticleGroupsSpTuere As Variant, _
                                specialArticleGroupsSpZargen As Variant, specialKnauf As Variant, specialGipsplatten As Variant, _
                                specialKühlschränke As Variant)
                                
    Dim inputSheet As Worksheet
    Dim artikel As String
    Dim artikelGruppe As Long
    Dim marke As String
    Dim länge As Double
    Dim breite As Double
    Dim tiefe As Double
    Dim gewicht As Double
    Dim versandkostenkennzeichen As Integer
    Dim versandart As Integer

    Set inputSheet = ThisWorkbook.Sheets("Aktuelle Artikeldaten")

    ' Grunddaten auslesen
    artikel = Trim(inputSheet.Cells(rowIndex, 1).Value)
    artikelGruppe = CLng(Trim(inputSheet.Cells(rowIndex, 3).Value))
    marke = Trim(inputSheet.Cells(rowIndex, 4).Value)
    länge = val(inputSheet.Cells(rowIndex, 5).Value)
    breite = val(inputSheet.Cells(rowIndex, 6).Value)
    tiefe = val(inputSheet.Cells(rowIndex, 7).Value)
    gewicht = val(inputSheet.Cells(rowIndex, 8).Value)
    
    versandkostenkennzeichen = 0
    versandart = 0

    If IsEmpty(artikel) Or artikel = "" Then
        inputSheet.Cells(rowIndex, 10).Value = "Kein Artikel"
        inputSheet.Cells(rowIndex, 11).Value = "Kein Artikel"
        Exit Sub
    End If

    ' SpecialBrands Regel
    If IsInArray(marke, specialBrands) Then
        längste_seite = Application.WorksheetFunction.Max(länge, breite, tiefe)
        gurtmass = 2 * (länge + breite + tiefe - längste_seite) + längste_seite
    
        If gewicht <= 30 And _
            ((länge <= 98 And breite <= 60 And tiefe <= 60) Or _
            (breite <= 98 And länge <= 60 And tiefe <= 60) Or _
            (tiefe <= 98 And länge <= 60 And breite <= 60)) Then
            versandkostenkennzeichen = 51
            versandart = 10
        ElseIf gewicht <= 10 And längste_seite <= 250 And gurtmass <= 400 Then
            versandkostenkennzeichen = 52
            versandart = 20
        ElseIf gewicht <= 30 And längste_seite <= 200 And gurtmass <= 400 Then
            versandkostenkennzeichen = 52
            versandart = 30
        Else
            versandkostenkennzeichen = 98
            versandart = 30
        End If
        
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    Debug.Print "Checking specialMuster...."
    Debug.Print "Artikelgruppe, die geprüft wird: " & artikelGruppe
    If IsInArray(artikelGruppe, specialMuster) Then
        Debug.Print "Found in special Muster"
        längste_seite = Application.WorksheetFunction.Max(länge, breite, tiefe)
        gurtmass = 2 * (länge + breite + tiefe - längste_seite) + längste_seite
        
        If gewicht <= 30 And _
            ((länge <= 98 And breite <= 60 And tiefe <= 60) Or _
            (breite <= 98 And länge <= 60 And tiefe <= 60) Or _
            (tiefe <= 98 And länge <= 60 And breite <= 60)) Then
            versandkostenkennzeichen = 98
            versandart = 11
        ElseIf gewicht <= 10 And längste_seite <= 250 And gurtmass <= 400 Then
            versandkostenkennzeichen = 98
            versandart = 21
        ElseIf gewicht <= 30 And längste_seite <= 200 And gurtmass <= 400 Then
            versandkostenkennzeichen = 98
            versandart = 21
        Else
            versandkostenkennzeichen = 98
            versandart = 31
        End If
        
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    If IsInArray(artikelGruppe, specialArticleGroupsSpTuere) And IsInArray(marke, specialBrandsSpTuere) Then
        versandkostenkennzeichen = 36
        versandart = 31
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    If IsInArray(artikelGruppe, specialArticleGroupsSpZargen) And IsInArray(marke, specialBrandsSpTuere) Then
        versandkostenkennzeichen = 53
        versandart = 31
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    If IsInArray(artikelGruppe, specialGipsplatten) And IsInArray(marke, specialKnauf) Then
        versandkostenkennzeichen = 49
        versandart = 31
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    If IsInArray(artikelGruppe, specialArticleGroupsOnlyViertelpalette) Then
        versandkostenkennzeichen = 31
        versandart = 31
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    If IsInArray(artikelGruppe, specialArticleGroupsOnlyHalbpalette) Then
        versandkostenkennzeichen = 32
        versandart = 31
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    ' Prüfung auf spezielle Artikelgruppen WandFliesen
    If IsInArray(artikelGruppe, specialArticleGroupsWandFliesen) Then
        Debug.Print "Special Article Group Wandfliesen found (VAL3): " & artikelGruppe
        Call VAL3_BestimmeVersandkostenWandfliesen(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    ' Prüfung auf spezielle Artikelgruppen mit 400 CHF
    If IsInArray(artikelGruppe, specialArticleGroups400CHF) Then
        Debug.Print "Special Article Group 400 CHF found (VAL3): " & artikelGruppe
        Call VAL3_BestimmeVersandkostenSpezial400CHF(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    ' Prüfung auf spezielle Artikelgruppen mit 700 CHF
    If IsInArray(artikelGruppe, specialArticleGroups700CHF) Then
        Debug.Print "Special Article Group 700 CHF found (VAL3): " & artikelGruppe
        Call VAL3_BestimmeVersandkostenSpezial700CHF(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    ' Prüfung auf Sackware
    If IsInArray(artikelGruppe, specialArticleGroupsSackware) Then
        Debug.Print "Special Article Group Sackware found (VAL3): " & artikelGruppe
        Call VAL3_BestimmeVersandkostenSpezialSackware(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    'Prüfung auf spezielle Artikelgruppen minimum Viertelpalette
    If IsInArray(artikelGruppe, specialArticleGroupsViertelpalette) Then
        Debug.Print "Special Article Group Viertelpalette found (VAL3): " & artikelGruppe
        Call VAL3_BestimmeVersandkostenSpezialViertelpalette(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    'Prüfung auf spezielle Artikelgruppen minimum Palette
    If IsInArray(artikelGruppe, specialArticleGroupsPalette) Then
        Debug.Print "Special Article Group Palette found (VAL3): " & artikelGruppe
        Call VAL3_BestimmeVersandkostenSpezialPalette(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    'Prüfung auf spezielle Artikelgruppen Sp Markiesen
    If IsInArray(artikelGruppe, specialArticleGroupsSpMarkiesen) Then
        Debug.Print "Special Article Group Sp Markiesen found (VAL3): " & artikelGruppe
        Call VAL3_BestimmeVersandkostenSpMarkiesen(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    'Prüfung auf spezielle Artikelgruppe Sp Holz
    If IsInArray(artikelGruppe, specialArticleGroupsSpHolz) Then
        Debug.Print "Special Article Group Sp Holz found (VAL3): " & artikelGruppe
        Call VAL3_BestimmeVersandkostenSpHolz(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    'Prüfung auf spezielle Artikelgruppe Fliesen
    If IsInArray(artikelGruppe, specialArticleGroupsFliesen) Then
        Debug.Print "Special Article Group Fliesen found (VAL3): " & artikelGruppe
        Call VAL3_BestimmeVersandkostenFliesen(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    'Prüfung auf spezielle Artikelgruppe Badewannen
    If IsInArray(artikelGruppe, specialArticleGroupsBandewannen) Then
        Debug.Print "Special Article Group Badewannen found (VAL3): " & artikelGruppe
        Call VAL3_BestimmeVersandkostenBadewannen(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    'Prüfung auf spezielle Artikelgruppe Spiegel
    If IsInArray(artikelGruppe, specialArticleGroupsSpiegel) Then
        Debug.Print "Special Article Group Spiegel found (VAL3): " & artikelGruppe
        Call VAL3_BestimmeVersandkostenSpiegel(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    'Prüfung auf spezielle Artikelgruppe Kühlschränke
    If IsInArray(artikelGruppe, specialArticleGroupsSpiegel) Then
        Debug.Print "Special Article Group Kühlschränke found (VAL 3): " & artikelGruppe
        Call VAL3_BestimmeVersandkostenKühlschränke(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
        inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
        inputSheet.Cells(rowIndex, 11).Value = versandart
        Exit Sub
    End If

    ' Standard-Versandkostenlogik
    Debug.Print "Normal shipping logic applied. (VAL3)"
    Call VAL3_BestimmeVersandkosten(länge, breite, tiefe, gewicht, versandkostenkennzeichen, versandart)
    inputSheet.Cells(rowIndex, 10).Value = versandkostenkennzeichen
    inputSheet.Cells(rowIndex, 11).Value = versandart
End Sub

Sub MVZ_BestimmeVersandkosten(länge As Double, breite As Double, tiefe As Double, gewicht As Double, ByRef versandkostenkennzeichen As Integer, ByRef versandart As Integer)
    ' Versandkostenkennzeichen 10 und Versandart 10
    If gewicht <= 30 And ((länge <= 98 And breite <= 60 And tiefe <= 60) Or _
                          (breite <= 98 And länge <= 60 And tiefe <= 60) Or _
                          (tiefe <= 98 And länge <= 60 And breite <= 60)) Then
        versandkostenkennzeichen = 10
        versandart = 10
        Exit Sub
    End If
    
    ' Versandkostenkennzeichen 11 und Versandart 20
    längste_seite = Application.WorksheetFunction.Max(länge, breite, tiefe)
    gurtmass = 2 * (länge + breite + tiefe - längste_seite) + längste_seite

    ' Erste Regel: Max 10kg, längste Seite max 250cm, Gurtmaß max 400cm
    If gewicht <= 10 And längste_seite <= 250 And gurtmass <= 400 Then
        versandkostenkennzeichen = 11
        versandart = 20
        Exit Sub
    End If

    ' Zweite Regel: Max 30kg, längste Seite max 200cm, Gurtmaß max 400cm
    If gewicht <= 30 And längste_seite <= 200 And gurtmass <= 400 Then
        versandkostenkennzeichen = 11
        versandart = 20
        Exit Sub
    End If
    
    ' Versandkostenkennzeichen 31 und Versandart 30
    ' Erste Regel: 40 x 60 x 300 mit Gewicht <= 50
    ' Zweite Regel: 40 x 60 x 60 mit Gewicht <= 100
    If (länge <= 40 And breite <= 60 And tiefe <= 300 And gewicht <= 50) Or _
        (länge <= 40 And breite <= 300 And tiefe <= 60 And gewicht <= 50) Or _
        (länge <= 60 And breite <= 40 And tiefe <= 300 And gewicht <= 50) Or _
        (länge <= 60 And breite <= 300 And tiefe <= 40 And gewicht <= 50) Or _
        (länge <= 300 And breite <= 40 And tiefe <= 60 And gewicht <= 50) Or _
        (länge <= 300 And breite <= 60 And tiefe <= 40 And gewicht <= 50) Or _
        (länge <= 40 And breite <= 60 And tiefe <= 60 And gewicht <= 100) Or _
        (länge <= 60 And breite <= 40 And tiefe <= 60 And gewicht <= 100) Or _
        (länge <= 60 And breite <= 60 And tiefe <= 40 And gewicht <= 100) Then
        versandkostenkennzeichen = 31
        versandart = 30
        Exit Sub
    End If
    
    ' Versandkostenkennzeichen 32 und Versandart 30
    ' Erste Regel: 40 x 60 x 400 mit Gewicht <= 70
    ' Zweite Regel: 40 x 80 x 80 mit Gewicht <= 300
    If (länge <= 40 And breite <= 60 And tiefe <= 400 And gewicht <= 70) Or _
        (länge <= 40 And breite <= 400 And tiefe <= 60 And gewicht <= 70) Or _
        (länge <= 60 And breite <= 40 And tiefe <= 400 And gewicht <= 70) Or _
        (länge <= 60 And breite <= 400 And tiefe <= 40 And gewicht <= 70) Or _
        (länge <= 400 And breite <= 40 And tiefe <= 60 And gewicht <= 70) Or _
        (länge <= 400 And breite <= 60 And tiefe <= 40 And gewicht <= 70) Or _
        (länge <= 40 And breite <= 80 And tiefe <= 80 And gewicht <= 300) Or _
        (länge <= 80 And breite <= 40 And tiefe <= 80 And gewicht <= 300) Or _
        (länge <= 80 And breite <= 80 And tiefe <= 40 And gewicht <= 300) Then
        versandkostenkennzeichen = 32
        versandart = 30
        Exit Sub
    End If
    
    ' Versandkostenkennzeichen 30 und Versandart 30
    ' Erste Regel: 40 x 60 x 599 mit Gewicht <= 70
    ' Zweite Regel: 120 x 120 x 80 mit Gewicht <= 800
    If (länge <= 40 And breite <= 60 And tiefe <= 599 And gewicht <= 70) Or _
        (länge <= 40 And breite <= 599 And tiefe <= 60 And gewicht <= 70) Or _
        (länge <= 60 And breite <= 40 And tiefe <= 599 And gewicht <= 70) Or _
        (länge <= 60 And breite <= 599 And tiefe <= 40 And gewicht <= 70) Or _
        (länge <= 599 And breite <= 40 And tiefe <= 60 And gewicht <= 70) Or _
        (länge <= 599 And breite <= 60 And tiefe <= 40 And gewicht <= 70) Or _
        (länge <= 120 And breite <= 120 And tiefe <= 80 And gewicht <= 800) Or _
        (länge <= 80 And breite <= 120 And tiefe <= 120 And gewicht <= 800) Or _
        (länge <= 120 And breite <= 80 And tiefe <= 120 And gewicht <= 800) Then
        versandkostenkennzeichen = 30
        versandart = 30
        Exit Sub
    End If
    
    ' Versandkostenkennzeichen 33 und Versandart 30
    ' Erste Regel: 80 x 120 x 240 mit Gewicht <= 900
    ' Zweite Regel: 120 x 120 x 160 mit Gewicht <= 900
    If (länge <= 80 And breite <= 120 And tiefe <= 240 And gewicht <= 900) Or _
        (länge <= 80 And breite <= 240 And tiefe <= 80 And gewicht <= 900) Or _
        (länge <= 120 And breite <= 80 And tiefe <= 240 And gewicht <= 900) Or _
        (länge <= 120 And breite <= 240 And tiefe <= 80 And gewicht <= 900) Or _
        (länge <= 240 And breite <= 80 And tiefe <= 120 And gewicht <= 900) Or _
        (länge <= 240 And breite <= 120 And tiefe <= 80 And gewicht <= 900) Or _
        (länge <= 120 And breite <= 120 And tiefe <= 160 And gewicht <= 900) Or _
        (länge <= 160 And breite <= 120 And tiefe <= 120 And gewicht <= 900) Or _
        (länge <= 120 And breite <= 160 And tiefe <= 120 And gewicht <= 900) Then
        versandkostenkennzeichen = 33
        versandart = 30
        Exit Sub
    End If

    ' Versandkostenkennzeichen 64 und Versandart 30
    If (länge = 80 And breite = 360 And tiefe = 200 And gewicht = 1300) Or _
        (länge = 80 And breite = 200 And tiefe = 360 And gewicht = 1300) Or _
        (länge = 360 And breite = 80 And tiefe = 200 And gewicht = 1300) Or _
        (länge = 200 And breite = 80 And tiefe = 360 And gewicht = 1300) Or _
        (länge = 360 And breite = 200 And tiefe = 80 And gewicht = 1300) Or _
        (länge = 200 And breite = 360 And tiefe = 80 And gewicht = 1300) Then
        versandkostenkennzeichen = 64
        versandart = 30
        Exit Sub
    End If

    ' Versandkostenkennzeichen 65 und Versandart 30
    If (länge <= 2400 And breite <= 480 And tiefe <= 320 And gewicht <= 3200) Or _
        (länge <= 2400 And breite <= 320 And tiefe <= 480 And gewicht <= 3200) Or _
        (länge <= 480 And breite <= 2400 And tiefe <= 320 And gewicht <= 3200) Or _
        (länge <= 320 And breite <= 2400 And tiefe <= 480 And gewicht <= 3200) Or _
        (länge <= 480 And breite <= 320 And tiefe <= 2400 And gewicht <= 3200) Or _
        (länge <= 320 And breite <= 480 And tiefe <= 2400 And gewicht <= 3200) Then
        versandkostenkennzeichen = 65
        versandart = 30
        Exit Sub
    End If

    ' Versandkostenkennzeichen 66 und Versandart 30
    If (länge <= 3000 And breite <= 600 And tiefe <= 400 And gewicht <= 4000) Or _
        (länge <= 3000 And breite <= 400 And tiefe <= 600 And gewicht <= 4000) Or _
        (länge <= 600 And breite <= 3000 And tiefe <= 400 And gewicht <= 4000) Or _
        (länge <= 400 And breite <= 3000 And tiefe <= 600 And gewicht <= 4000) Or _
        (länge <= 600 And breite <= 400 And tiefe <= 3000 And gewicht <= 4000) Or _
        (länge <= 400 And breite <= 600 And tiefe <= 3000 And gewicht <= 4000) Then
        versandkostenkennzeichen = 66
        versandart = 30
        Exit Sub
    End If

    ' Versandkostenkennzeichen 67 und Versandart 30
    If (länge <= 3600 And breite <= 720 And tiefe <= 480 And gewicht <= 4800) Or _
        (länge <= 3600 And breite <= 480 And tiefe <= 720 And gewicht <= 4800) Or _
        (länge <= 720 And breite <= 3600 And tiefe <= 480 And gewicht <= 4800) Or _
        (länge <= 480 And breite <= 3600 And tiefe <= 720 And gewicht <= 4800) Or _
        (länge <= 720 And breite <= 480 And tiefe <= 3600 And gewicht <= 4800) Or _
        (länge <= 480 And breite <= 720 And tiefe <= 3600 And gewicht <= 4800) Then
        versandkostenkennzeichen = 67
        versandart = 30
        Exit Sub
    End If
End Sub

Sub MVZ_BestimmeVersandkostenSpezial400CHF(länge As Double, breite As Double, tiefe As Double, gewicht As Double, ByRef versandkostenkennzeichen As Integer, ByRef versandart As Integer)
        ' Versandkostenkennzeichen 10 und Versandart 10
        If gewicht <= 30 And ((länge <= 98 And breite <= 60 And tiefe <= 60) Or _
                            (breite <= 98 And länge <= 60 And tiefe <= 60) Or _
                            (tiefe <= 98 And länge <= 60 And breite <= 60)) Then
            versandkostenkennzeichen = 10
            versandart = 10
            Exit Sub
        End If
    
        ' Versandkostenkennzeichen 11 und Versandart 20
        längste_seite = Application.WorksheetFunction.Max(länge, breite, tiefe)
        gurtmass = 2 * (länge + breite + tiefe - längste_seite)
        If längste_seite <= 250 And gurtmass <= 400 And gewicht <= 30 Then
            versandkostenkennzeichen = 11
            versandart = 20
            Exit Sub
        End If

        ' Versandkostenkennzeichen 61 und Versandart 30
        ' Erste Regel: 40 x 60 x 300 mit Gewicht <= 50
        ' Zweite Regel: 40 x 60 x 60 mit Gewicht <= 100
        If (länge <= 40 And breite <= 60 And tiefe <= 300 And gewicht <= 50) Or _
            (länge <= 40 And breite <= 300 And tiefe <= 60 And gewicht <= 50) Or _
            (länge <= 60 And breite <= 40 And tiefe <= 300 And gewicht <= 50) Or _
            (länge <= 60 And breite <= 300 And tiefe <= 40 And gewicht <= 50) Or _
            (länge <= 300 And breite <= 40 And tiefe <= 60 And gewicht <= 50) Or _
            (länge <= 300 And breite <= 60 And tiefe <= 40 And gewicht <= 50) Or _
            (länge <= 40 And breite <= 60 And tiefe <= 60 And gewicht <= 100) Or _
            (länge <= 60 And breite <= 40 And tiefe <= 60 And gewicht <= 100) Or _
            (länge <= 60 And breite <= 60 And tiefe <= 40 And gewicht <= 100) Then
            versandkostenkennzeichen = 61
            versandart = 30
            Exit Sub
        End If

        ' Versandkostenkennzeichen 62 und Versandart 30
        ' Erste Regel: 40 x 60 x 400 mit Gewicht <= 70
        ' Zweite Regel: 40 x 80 x 80 mit Gewicht <= 300
        If (länge <= 40 And breite <= 60 And tiefe <= 400 And gewicht <= 70) Or _
            (länge <= 40 And breite <= 400 And tiefe <= 60 And gewicht <= 70) Or _
            (länge <= 60 And breite <= 40 And tiefe <= 400 And gewicht <= 70) Or _
            (länge <= 60 And breite <= 400 And tiefe <= 40 And gewicht <= 70) Or _
            (länge <= 400 And breite <= 40 And tiefe <= 60 And gewicht <= 70) Or _
            (länge <= 400 And breite <= 60 And tiefe <= 40 And gewicht <= 70) Or _
            (länge <= 40 And breite <= 80 And tiefe <= 80 And gewicht <= 300) Or _
            (länge <= 80 And breite <= 40 And tiefe <= 80 And gewicht <= 300) Or _
            (länge <= 80 And breite <= 80 And tiefe <= 40 And gewicht <= 300) Then
            versandkostenkennzeichen = 62
            versandart = 30
            Exit Sub
        End If

        ' Versandkostenkennzeichen 63 und Versandart 30
        ' Erste Regel: 40 x 60 x 599 mit Gewicht <= 70
        ' Zweite Regel: 120 x 120 x 80 mit Gewicht <= 800
        If (länge <= 40 And breite <= 60 And tiefe <= 599 And gewicht <= 70) Or _
            (länge <= 40 And breite <= 599 And tiefe <= 60 And gewicht <= 70) Or _
            (länge <= 60 And breite <= 40 And tiefe <= 599 And gewicht <= 70) Or _
            (länge <= 60 And breite <= 599 And tiefe <= 40 And gewicht <= 70) Or _
            (länge <= 599 And breite <= 40 And tiefe <= 60 And gewicht <= 70) Or _
            (länge <= 599 And breite <= 60 And tiefe <= 40 And gewicht <= 70) Or _
            (länge <= 480 And breite <= 720 And tiefe <= 3600 And gewicht <= 900) Or _
            (länge <= 480 And breite <= 3600 And tiefe <= 720 And gewicht <= 900) Or _
            (länge <= 720 And breite <= 480 And tiefe <= 3600 And gewicht <= 900) Or _
            (länge <= 720 And breite <= 3600 And tiefe <= 480 And gewicht <= 900) Or _
            (länge <= 3600 And breite <= 480 And tiefe <= 720 And gewicht <= 900) Or _
            (länge <= 3600 And breite <= 720 And tiefe <= 480 And gewicht <= 900) Then
            versandkostenkennzeichen = 63
            versandart = 30
            Exit Sub
        End If
End Sub

Sub MVZ_BestimmeVersandkostenSpezial700CHF(länge As Double, breite As Double, tiefe As Double, gewicht As Double, ByRef versandkostenkennzeichen As Integer, ByRef versandart As Integer)
        ' Versandkostenkennzeichen 10 und Versandart 10
        If gewicht <= 30 And ((länge <= 98 And breite <= 60 And tiefe <= 60) Or _
                            (breite <= 98 And länge <= 60 And tiefe <= 60) Or _
