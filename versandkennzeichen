Sub FFDL_BestimmeVersandkostenSpiegel(länge As Double, breite As Double, tiefe As Double, gewicht As Double, ByRef versandkostenkennzeichen As Integer, ByRef versandart As Integer)
    ' Versandkostenkennzeichen 58 und Versandart 10
    If (länge <= 98 And breite <= 60 And tiefe <= 60 And gewicht <= 2) Or _
        (länge <= 98 And breite <= 60 And tiefe <= 60 And gewicht <= 2) Or _
        (länge <= 60 And breite <= 98 And tiefe <= 60 And gewicht <= 2) Or _
        (länge <= 60 And breite <= 98 And tiefe <= 60 And gewicht <= 2) Or _
        (länge <= 60 And breite <= 60 And tiefe <= 98 And gewicht <= 2) Or _
        (länge <= 60 And breite <= 60 And tiefe <= 98 And gewicht <= 2) Then
        versandkostenkennzeichen = 58
        versandart = 10
        Exit Sub
    End If

    ' Versandkostenkennzeichen 55 und Versandart 30
    If (länge <= 40 And breite <= 60 And tiefe <= 60 And gewicht <= 100) Or _
        (länge <= 40 And breite <= 60 And tiefe <= 60 And gewicht <= 100) Or _
        (länge <= 60 And breite <= 40 And tiefe <= 60 And gewicht <= 100) Or _
        (länge <= 60 And breite <= 40 And tiefe <= 60 And gewicht <= 100) Or _
        (länge <= 60 And breite <= 60 And tiefe <= 40 And gewicht <= 100) Or _
        (länge <= 60 And breite <= 60 And tiefe <= 40 And gewicht <= 100) Then
        versandkostenkennzeichen = 55
        versandart = 30
        Exit Sub
    End If

    ' Versandkostenkennzeichen 56 und Versandart 30
    If (länge <= 120 And breite <= 120 And tiefe <= 80 And gewicht <= 800) Or _
        (länge <= 120 And breite <= 120 And tiefe <= 80 And gewicht <= 800) Or _
        (länge <= 80 And breite <= 120 And tiefe <= 120 And gewicht <= 800) Or _
        (länge <= 80 And breite <= 120 And tiefe <= 120 And gewicht <= 800) Or _
        (länge <= 120 And breite <= 80 And tiefe <= 120 And gewicht <= 800) Or _
        (länge <= 120 And breite <= 80 And tiefe <= 120 And gewicht <= 800) Then
        versandkostenkennzeichen = 56
        versandart = 30
        Exit Sub
    End If

    ' Versandkostenkennzeichen 13 und Versandart 30
    ' Erste Regel: 80 x 240 x 120 mit Gewicht <= 900
    ' Zweite Regel: 160 x 120 x 120 mit Gewicht <= 900
    If (länge <= 80 And breite <= 120 And tiefe <= 240 And gewicht <= 900) Or _
        (länge <= 80 And breite <= 240 And tiefe <= 120 And gewicht <= 900) Or _
        (länge <= 120 And breite <= 80 And tiefe <= 240 And gewicht <= 900) Or _
        (länge <= 120 And breite <= 240 And tiefe <= 80 And gewicht <= 900) Or _
        (länge <= 240 And breite <= 80 And tiefe <= 120 And gewicht <= 900) Or _
        (länge <= 240 And breite <= 120 And tiefe <= 80 And gewicht <= 900) Or _
        (länge <= 160 And breite <= 120 And tiefe <= 120 And gewicht <= 900) Or _
        (länge <= 120 And breite <= 160 And tiefe <= 120 And gewicht <= 900) Or _
        (länge <= 120 And breite <= 120 And tiefe <= 160 And gewicht <= 900) Then
        versandkostenkennzeichen = 13
        versandart = 30
        Exit Sub
    End If
End Sub

